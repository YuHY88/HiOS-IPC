/*                                 Bit Field Define For Alarm Type Define                                      */
/*--------------------------------------------------------------------------*/

/* Alarm Type Bit Mask */
#define GPN_ALM_TYPE_MASK		0xFFFF0000
/* Alarm Sub Type Bit Mask */
#define GPN_ALM_SUB_TYPE_MASK	0x0000FFFF
#define IS_GPN_ALM_SUB_TYPE(almSubType)	(((almSubType) & GPN_ALM_SUB_TYPE_MASK) != 0)
#define IS_GPN_ALM_TYPE(almType)	(((almType) & GPN_ALM_SUB_TYPE_MASK) == 0)	


