/*--------------------------------------------------------------------------*/
/*                              GPN Alarm Type SubType's Page(s) Define                                   */
/*--------------------------------------------------------------------------*/
/* Alarm Type Bit Express : Page Info Bit Local */
#define GPN_ALM_IN_PAGE_BITP	0x0FFFFFFF
/* Alarm Type Bit Express : Max Alarm Type Num In One Page */
#define GPN_ALM_PAGE_BIT_SIZE	28
/* Alarm Type Bit Express : Page Info Bit Mask */
#define GPN_ALM_PAGE_MASK		0xF0000000
/* Alarm Type Bit Express : Page Info Bit Num */
#define GPN_ALM_PAGE_SIZE		4
/* Alarm Type Bit Express : Max Page Num */
#define GPN_ALM_PAGE_NUM		16
/* Alarm Type Bit Express : Page Value */
#define GPN_ALM_PAGE_VALE(almBitId) (((almBitId)&(GPN_ALM_PAGE_MASK)>>GPN_ALM_PAGE_BIT_SIZE)
/* Sys Support Max Alarm Num In One Alarm Type For Alarm Restrain Within Species (29 * 8 = 232)*/
#define GPN_ALM_MAX_REST_ALM	(GPN_ALM_PAGE_BIT_SIZE * GPN_ALM_PAGE_NUM)
