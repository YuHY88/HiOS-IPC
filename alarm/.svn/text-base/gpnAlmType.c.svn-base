UINT32 gpnAlmRestSpecialArrayClearBits(UINT32 *array, UINT32 arraySize, UINT32 bitNum, UINT32 offset)
{
	UINT32 wholePag;
	UINT32 bits;
	UINT32 temp[GPN_ALM_PAGE_NUM];
	UINT32 i;
	
	if(	(array == NULL)||(arraySize > GPN_ALM_PAGE_NUM)||
		((bitNum + offset) > arraySize*GPN_ALM_PAGE_BIT_SIZE))
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : ClearBits severity error BitNum %d offset %d\n\r",\
			__FUNCTION__, bitNum, offset);
		gpnLog(GPN_LOG_L_ALERT, "%s : ClearBits severity error BitNum %d offset %d\n\r",\
			__FUNCTION__, bitNum, offset);
		return GPN_ALM_GEN_NO;
	}
	else
	{
		gpnAlmRestSpecialArrayInit(temp, GPN_ALM_PAGE_NUM);
		
		wholePag = (bitNum + offset) / GPN_ALM_PAGE_BIT_SIZE;
		bits = (bitNum + offset) % GPN_ALM_PAGE_BIT_SIZE;

		for(i=0; i<wholePag; i++)
		{
			temp[i] &= (~GPN_ALM_IN_PAGE_BITP);
		}
		temp[i] &= (~((1U << bits)-1));

		wholePag = (offset) / GPN_ALM_PAGE_BIT_SIZE;
		bits = (offset) % GPN_ALM_PAGE_BIT_SIZE;

		for(i=0; i<wholePag; i++)
		{
			temp[i] |= (GPN_ALM_IN_PAGE_BITP);
		}
		temp[i] |= (((1U << bits)-1));
		
		for(i=0; i<GPN_ALM_PAGE_NUM; i++)
		{
			array[i] &= temp[i];
		}
		return GPN_ALM_GEN_YES;
	}
}
