UINT32 gpnAlmpRestRulerBeRestAlmReadIn(sysSupAlmRestTpTemp *pAlmRestTpTemp, restrainTemp *pRestrainTemp,
	formRestrainShip *pRestShipPoolHead, formRestrainShip **ppRestShipBeRest)
{
	formRestrainShip *pRestShip = NULL;
	formRestrainShip *pRestShipBeRest = NULL;
	UINT32 i;
	UINT32 j;

	/* ####  beRest find */
	pRestShip = pRestShipPoolHead;
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++,pRestShip++)
	{
		/* check if alarm type in restrain rule already read in restShip */
		if(pRestShip->almSubTp == 0)
		{
			/* restShip is empty, record beRestAlm in restShipPool */
			pRestShip->almSubTp = pRestrainTemp->beResAlmTp;
			pRestShip->level = 1;
			gpnAlmRestSpecialArrayInit(pRestShip->restMask, GPN_ALM_PAGE_NUM);
			pRestShip->bitPosition = 0;
			/* restShipPool's almType info change for new almType add */
			pRestShipBeRest = pRestShipPoolHead;
			for(j=0; j<pAlmRestTpTemp->almRestRuleNum; j++,pRestShipBeRest++)
			{	
				if(pRestShipBeRest->almSubTp == 0)
				{
					/* in restShipPool bottom, break */
					break;
				}
				else
				{
					if(pRestShipBeRest->almSubTp != pRestrainTemp->beResAlmTp)
					{
						/* add new almType in poll, so mov left */
						pRestShipBeRest->bitPosition++;
						gpnAlmRestSpecialArrayLeftMov(pRestShipBeRest->restMask, GPN_ALM_PAGE_NUM, 1, 1);
					}
				}
			}

			/* set to new add restShip almType */
			pRestShipBeRest = pRestShip;
			break;
		}
		else if(pRestShip->almSubTp == pRestrainTemp->beResAlmTp)
		{
			/* set to old same restShip almType */
			pRestShipBeRest = pRestShip;
			break;		
		}
		else
		{
			/*
				if restShip' almType not same with restTemp's almType, 
				restShip not in bottom at same time, continue next restShip
			*/
		}
	}
	
	/* "==" or "new add" is must case, means pRestShipBeRest must be in restShipPoll */
	if(i >= pAlmRestTpTemp->almRestRuleNum)
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : beResAlmTp search num err\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : beResAlmTp search num err\n\r",\
			__FUNCTION__);
		return GPN_ALM_GEN_ERR;
	}

	/* trans beRest for canRest proc */
	*ppRestShipBeRest = pRestShipBeRest;

	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmpRestRulerCanRestAlmReadIn(sysSupAlmRestTpTemp *pAlmRestTpTemp, restrainTemp *pRestrainTemp,
	formRestrainShip *pRestShipPoolHead, formRestrainShip *pRestShipBeRest, UINT32 *pTopLevelNow, UINT32 *pTopLevelMach)
{
	formRestrainShip *pRestShipPool;
	formRestrainShip *pRestShipToRest;
	UINT32 cmpMask[GPN_ALM_PAGE_NUM];
	UINT32 tempMask[GPN_ALM_PAGE_NUM];
	UINT32 beRestMask[GPN_ALM_PAGE_NUM];
	UINT32 beRestMark[GPN_ALM_PAGE_NUM];
	UINT32 toRestMask[GPN_ALM_PAGE_NUM];
	UINT32 nowBeRestLevel;
	UINT32 topLevelNow;
	UINT32 topLevelMach;
	UINT32 i;
	UINT32 j;
	UINT32 k;

	topLevelNow = *pTopLevelNow;
	topLevelMach = *pTopLevelMach;
	
	/* ####  canRest find */
	pRestShipToRest = pRestShipPoolHead;
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++,pRestShipToRest++)
	{
		if(pRestrainTemp->whoResMe == 0)
		{
			/* no almType rest this one, canRest find over */
			pRestShipBeRest->level = topLevelNow;
			break;
		}
	
		/* check if alarm type in restrain rule already read in restShip */
		if(pRestShipToRest->almSubTp == 0)
		{
			/* record canRest almType in restShipPool */
			pRestShipToRest->almSubTp = pRestrainTemp->whoResMe;
			/*
				this would make tow almType have same bitPosition
				and canRest left than beRest in bitPosition
			*/
			pRestShipToRest->bitPosition = pRestShipBeRest->bitPosition + 1;
			gpnAlmRestSpecialArrayInit(pRestShipToRest->restMask, GPN_ALM_PAGE_NUM);
			/* clear canRest's special bit, because beRest bit is know */
			gpnAlmRestSpecialArrayClearBit(pRestShipToRest->restMask, GPN_ALM_PAGE_NUM, pRestShipBeRest->bitPosition);
			/*canRest almType have up level than beRest almType */
			pRestShipToRest->level = pRestShipBeRest->level + 1;
			if(pRestShipToRest->level > topLevelNow)
			{
				/* renew topLevel */
				topLevelNow = pRestShipToRest->level;
			}			
			/* add canRest almType in restShipPool, renew restShipPool for this add */
			pRestShipPool = pRestShipPoolHead;
			for(j=0; j<pAlmRestTpTemp->almRestRuleNum;j++,pRestShipPool++)
			{
				/* restShipPoll bottom */
				if(pRestShipPool->almSubTp == 0)
				{
					break;
				}
	
				/*
					these almTypes in restShipPool should renew:
					1 bitPosition left than beRest almType;
					2 and not canRest almType;
				*/
				if( (pRestShipPool->bitPosition > pRestShipBeRest->bitPosition) &&\
					(pRestShipPool->almSubTp != pRestrainTemp->whoResMe) )
				{
					/*test
					formRestrainShip *testtest;
					UINT32 test;
					testtest = pRestShipPoolHead;
					for(test=0;test<pgSysSupAlmRestTpTemp->almRestRuleNum;test++,testtest++)
					{
						if(testtest->almSubTp == 0)
						{
							break;
						}
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%08x %02d %02d %08x %08x %08x %08x\n",testtest->almSubTp,testtest->level,testtest->bitPosition,\
							testtest->restMask[3],testtest->restMask[2],testtest->restMask[1],testtest->restMask[0]);
					}*/
					
					/* left mov bitP, make position for new add canRest almType */
					pRestShipPool->bitPosition++;
	
					/* left bit modify */
					gpnAlmRestSpecialArrayCpyOpt(tempMask, pRestShipPool->restMask, GPN_ALM_PAGE_NUM);
					gpnAlmRestSpecialArrayLeftMov(tempMask, GPN_ALM_PAGE_NUM, 1, 1);
					/* clear this alarm restMask right base canRest bitPosition */
					gpnAlmRestSpecialArrayClearBits(tempMask, GPN_ALM_PAGE_NUM, pRestShipToRest->bitPosition, 0);
					/* set this(temp) alarm restMask canRest bitPosition as 1 */
					gpnAlmRestSpecialArraySetBit(tempMask, GPN_ALM_PAGE_NUM, pRestShipToRest->bitPosition);
					/* clear this(real) alarm restMask right base canRest bitPosition */
					gpnAlmRestSpecialArrayClearBits(pRestShipPool->restMask, GPN_ALM_PAGE_NUM,\
						(GPN_ALM_PAGE_NUM*GPN_ALM_PAGE_BIT_SIZE-pRestShipToRest->bitPosition),\
						pRestShipToRest->bitPosition);
					gpnAlmRestSpecialArrayBitWithOrOpt(pRestShipPool->restMask, tempMask, GPN_ALM_PAGE_NUM);
				}
			}
			break;
		}
		else if(pRestShipToRest->almSubTp == pRestrainTemp->whoResMe)
		{
			/* find canRest almType in restShipPool, clear beRest almType bitP */
			gpnAlmRestSpecialArrayClearBit(pRestShipToRest->restMask, GPN_ALM_PAGE_NUM, pRestShipBeRest->bitPosition);
			/*
				if canRest almType's level low or equal than beRest almType, 
				increase canRest almType and all up level almType 
			*/
			if(pRestShipToRest->level <= pRestShipBeRest->level)
			{
				/*
					here logic :
					if A can restrain B, and A has a low or equal level, 
					then A must increase it's level up to B, and these alm
					who can restrain A must all increase these level; check
					A -> B -> C -> A (restrain loop) at same time
				*/
				gpnAlmRestSpecialArrayClear(beRestMask, GPN_ALM_PAGE_NUM);
				gpnAlmRestSpecialArraySetBit(beRestMask, GPN_ALM_PAGE_NUM, pRestShipBeRest->bitPosition);
				
				gpnAlmRestSpecialArrayClear(beRestMark, GPN_ALM_PAGE_NUM);
				gpnAlmRestSpecialArraySetBit(beRestMark, GPN_ALM_PAGE_NUM, pRestShipBeRest->bitPosition);
				
				gpnAlmRestSpecialArrayClear(toRestMask, GPN_ALM_PAGE_NUM);
				gpnAlmRestSpecialArraySetBit(toRestMask, GPN_ALM_PAGE_NUM, pRestShipToRest->bitPosition);
				
				nowBeRestLevel = pRestShipBeRest->level;
				/*共查询的次数:层次数小于被抑制告警的能抑制告警层次数-最高层次数+1*/
				for(j=pRestShipToRest->level;j<=topLevelNow;j++)
				{
					gpnAlmRestSpecialArrayClear(tempMask, GPN_ALM_PAGE_NUM);
					pRestShipPool = pRestShipPoolHead;
					for(k=0; k<pAlmRestTpTemp->almRestRuleNum; (k++,pRestShipPool++))
					{	
						if(pRestShipPool->almSubTp == 0)
						{
							break;
						}
						else
						{
							/*pRestShip表中找到的记录显示其可以抑制X，且层次数小于等于X，故其层次数可以升高*/
							gpnAlmRestSpecialArrayCpyOpt(cmpMask, pRestShipPool->restMask, GPN_ALM_PAGE_NUM);
							gpnAlmRestSpecialArrayBitWithAndOpt(cmpMask, beRestMask, GPN_ALM_PAGE_NUM);
							if( (gpnAlmRestSpecialArrayCmpOpt(cmpMask, beRestMask, GPN_ALM_PAGE_NUM) == GPN_ALM_GEN_ERR) &&
								((pRestShipPool->level) <= nowBeRestLevel) )
							{
								/*能抑制告警层次升高*/
								pRestShipPool->level = nowBeRestLevel+1;
								/*记录能抑制告警比特掩码，供下次FOR查找谁是这些能抑制告警的能抑制告警(当然层次也低)*/
								gpnAlmRestSpecialArraySetBit(tempMask, GPN_ALM_PAGE_NUM, pRestShipPool->bitPosition);	
								if(pRestShipPool->level > topLevelNow)
								{
									topLevelMach = pRestShipPool->level;
								}
							}
						}
					}
	
					/*在此检测抑制成环的问题，原理:如果在这种我抑制得了你，且我的层次数小于你，我改变我的层
					次数为你的层次数加1(即A->B且A.level<=B.level,则A.level=B.level+1)的处理中，又出现你抑制
					了能抑制我的告警，则就成环了(如之前A->B,C->A,则B与A都为被抑制过的告警，如果现在出现A->C,
					则不是成了C->A->B/C),那么A/B/C永远不会出现，因为自己抑制自己，实际不存在这样的逻辑*/
					gpnAlmRestSpecialArrayBitWithOrOpt(toRestMask, tempMask, GPN_ALM_PAGE_NUM);
					
					gpnAlmRestSpecialArrayCpyOpt(cmpMask, toRestMask, GPN_ALM_PAGE_NUM);
					gpnAlmRestSpecialArrayBitWithAndOpt(cmpMask, beRestMark, GPN_ALM_PAGE_NUM);
					if(gpnAlmRestSpecialArrayIsZero(cmpMask, GPN_ALM_PAGE_NUM) != GPN_ALM_GEN_YES)
					/*toRestMask |= tempMask;
					if((toRestMask & beRestMark) != 0)*/
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : beResAlmTp search loop err\n\r",\
							__FUNCTION__);
						gpnLog(GPN_LOG_L_ALERT, "%s : beResAlmTp search loop err\n\r",\
							__FUNCTION__);
						return GPN_ALM_GEN_ERR;
					}
	
					gpnAlmRestSpecialArrayCpyOpt(beRestMask, tempMask, GPN_ALM_PAGE_NUM);
					nowBeRestLevel++;
					topLevelNow = topLevelMach;
					if(topLevelNow > GPN_ALM_MAX_REST_ALM)
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : rest rule err topLevelNow = %d\n\r",\
							__FUNCTION__, topLevelNow);
						gpnLog(GPN_LOG_L_ALERT, "%s : rest rule err topLevelNow = %d\n\r",\
							__FUNCTION__, topLevelNow);
						return GPN_ALM_GEN_ERR;
					}
				}
				
			}
			break;
		}
		else
		{
			/*
				if restShip' almType not same with restTemp's almType, 
				restShip not in bottom at same time, continue next restShip
			*/
		}
	}
	
	/* "==" or "new add" is must case, means pRestShipBeRest must be in restShipPoll */
	if(i >= pAlmRestTpTemp->almRestRuleNum)
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : whoResMe search err\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : whoResMe search err\n\r",\
			__FUNCTION__);
		return GPN_ALM_GEN_ERR;
	}

	*pTopLevelNow = topLevelNow;
	*pTopLevelMach = topLevelMach;
	
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmpRestShipPoolReorderBaseLevel(sysSupAlmRestTpTemp *pAlmRestTpTemp, formRestrainShip *pRestShipPoolHead)
{
	formRestrainShip SWAPrestShip;
	formRestrainShip *pRestShipA;
	formRestrainShip *pRestShipB;
	UINT32 i;
	UINT32 j;
	
	/* first, reorder base level */
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++)
	{
		SWAPrestShip.almSubTp = 0;
		pRestShipA = pRestShipPoolHead;
		pRestShipB = pRestShipPoolHead + 1;
		if((pRestShipB->almSubTp == 0)||(pRestShipA->almSubTp == 0))
		{
			break;
		}
		for(j=0; j<pAlmRestTpTemp->almRestRuleNum; j++)
		{
			if((pRestShipB->almSubTp == 0)||(pRestShipA->almSubTp == 0))
			{
				break;
			}
			/* put hight level behind */
			if(pRestShipA->level > pRestShipB->level)
			{
				SWAPrestShip = *pRestShipA;
				*pRestShipA = *pRestShipB;
				*pRestShipB = SWAPrestShip;
			}
			pRestShipA++;
			pRestShipB++;
		}
		/* reorder may over before all cyc done */
		if(SWAPrestShip.almSubTp == 0)
		{
			break;
		}
	}
	
	/*test after reorder base level 
	debugGpnAlmRestShipPool(pgSysSupAlmRestTpTemp, pRestShipPoolHead);*/
	
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmpRestShipPoolReorderBaseBitPosition(sysSupAlmRestTpTemp *pAlmRestTpTemp, formRestrainShip *pRestShipPoolHead)
{
	formRestrainShip SWAPrestShip;
	formRestrainShip *pRestShipA;
	formRestrainShip *pRestShipB;
	UINT32 i;
	UINT32 j;
	
	/* first, reorder base level */
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++)
	{
		SWAPrestShip.almSubTp = 0;
		pRestShipA = pRestShipPoolHead;
		pRestShipB = pRestShipPoolHead + 1;
		if((pRestShipB->almSubTp == 0)||(pRestShipA->almSubTp == 0))
		{
			break;
		}
		for(j=0; j<pAlmRestTpTemp->almRestRuleNum; j++)
		{
			if((pRestShipB->almSubTp == 0)||(pRestShipA->almSubTp == 0))
			{
				break;
			}
			/* put hight level behind , then put high bit position behind */
			if( (pRestShipA->level == pRestShipB->level) &&\
				(pRestShipA->bitPosition < pRestShipB->bitPosition) )
			{
				SWAPrestShip = *pRestShipA;
				*pRestShipA = *pRestShipB;
				*pRestShipB = SWAPrestShip;
			}
			pRestShipA++;
			pRestShipB++;
		}
		/* reorder may over before all cyc done */
		if(SWAPrestShip.almSubTp == 0)
		{
			break;
		}
	}
	
	/*test after reorder base level 
	debugGpnAlmRestShipPool(pgSysSupAlmRestTpTemp, pRestShipPoolHead);*/
	
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmpRestShipPoolRestTransmit(sysSupAlmRestTpTemp *pAlmRestTpTemp, formRestrainShip *pRestShipPoolHead)
{
	formRestrainShip *pRestShip;
	formRestrainShip *pRestShipBeRest;
	UINT32 tempMask[GPN_ALM_PAGE_NUM];
	UINT32 cmpMask[GPN_ALM_PAGE_NUM];
	UINT32 i;
	UINT32 j;
	UINT32 k;

	pRestShip = pRestShipPoolHead;
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++,pRestShip++)
	{	
		if(pRestShip->almSubTp == 0)
		{
			break;
		}
		/* copy it can rest who, find who's rest capability */
		gpnAlmRestSpecialArrayCpyOpt(tempMask, pRestShip->restMask, GPN_ALM_PAGE_NUM);
		gpnAlmRestSpecialArrayBitReverseOpt(tempMask, GPN_ALM_PAGE_NUM);
		for(j=0; j<GPN_ALM_MAX_REST_ALM; j++)
		{
			/* who's rest capability check */
			if(gpnAlmRestSpecialArrayIsBitSet(tempMask, GPN_ALM_PAGE_NUM, j)==GPN_ALM_GEN_YES)
			{
				/* find rest capability vs almType */
				pRestShipBeRest = pRestShipPoolHead;
				for(k=0; k<pAlmRestTpTemp->almRestRuleNum; k++,pRestShipBeRest++)
				{
					if(pRestShip->almSubTp == 0)
					{
						break;
					}
					
					if(pRestShipBeRest->bitPosition == j)
					{
						/* rest transmit */
						gpnAlmRestSpecialArrayCpyOpt(cmpMask, pRestShipBeRest->restMask, GPN_ALM_PAGE_NUM);
						gpnAlmRestSpecialArrayBitReverseOpt(cmpMask, GPN_ALM_PAGE_NUM);
						gpnAlmRestSpecialArrayBitWithAndOpt(cmpMask, pRestShip->restMask, GPN_ALM_PAGE_NUM);
						if(gpnAlmRestSpecialArrayIsZero(cmpMask, GPN_ALM_PAGE_NUM) != GPN_ALM_GEN_YES)
						{
							/* redo rest loop check */
							if(gpnAlmRestSpecialArrayIsBitSet(pRestShipBeRest->restMask, GPN_ALM_PAGE_NUM, j)==GPN_ALM_GEN_YES)
							{
								gpnAlmRestSpecialArrayBitWithAndOpt(pRestShip->restMask, pRestShipBeRest->restMask, GPN_ALM_PAGE_NUM);
							}
							else
							{
								GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : in rest pass,beResAlmTp happened almType %08x i %d\n\r",\
									__FUNCTION__, pRestShip->almSubTp, i);
								gpnLog(GPN_LOG_L_ALERT, "%s : in rest pass,beResAlmTp happened almType %08x i %d\n\r",\
									__FUNCTION__, pRestShip->almSubTp, i);
								return GPN_ALM_GEN_ERR;
							}
						}
						break;
					}
				}
			}
		}
	}

	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmRestSubTypeInfoFinish(sysSupAlmRestTpTemp *pAlmRestTpTemp, formRestrainShip *pRestShipPool)
{
	formRestrainShip *pRestShip;
	stAlmSubTpNode *pAlmSubTpNode;
	stAlmTpNode *pAlmTpNode;
	stAlmTypeDef *pAlmType;
	UINT32 almSubTypeNum;
	UINT32 almTypeNum;
	UINT32 i;
	
	pRestShip = pRestShipPool;
	for(i=0;i<pAlmRestTpTemp->almRestRuleNum;i++,pRestShip++)
	{
		/* almRestRuleNum <= restShipPoolMax */
		if(pRestShip->almSubTp == 0)
		{
			break;
		}
	
		/* finish subType info */
		almTypeNum = 0;
		pAlmTpNode = (stAlmTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genAlmTypeQuen));
		while((pAlmTpNode != NULL)&&(almTypeNum<pgstAlmTypeWholwInfo->genAlmTypeNum))
		{
			pAlmType = pAlmTpNode->pAlmTpStr;
			if(((pAlmType->almType & GPN_ALM_TYPE_MASK) == (pRestShip->almSubTp & GPN_ALM_TYPE_MASK)) &&\
				(IS_GPN_ALM_SUB_TYPE(pRestShip->almSubTp)))
			{
				almSubTypeNum = 0;
				pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmType->genAlmSubTypeCharaQuen));
				while((pAlmSubTpNode != NULL)&&(almSubTypeNum<pAlmType->genAlmNumInType))
				{
					if(pAlmSubTpNode->pAlmSubTpStr->almSubType == pRestShip->almSubTp)
					{
						/* restrain info set for subType of restClass */
						pAlmSubTpNode->pAlmSubTpStr->almUseBitPosition = pRestShip->bitPosition;
						gpnAlmRestSpecialArrayBitIdGet(&(pAlmSubTpNode->pAlmSubTpStr->almUseIdentify), GPN_ALM_PAGE_NUM, pRestShip->bitPosition);
						pAlmSubTpNode->pAlmSubTpStr->almLevel = pRestShip->level;
						gpnAlmRestSpecialArrayCpyOpt(pAlmSubTpNode->pAlmSubTpStr->restrainMask, pRestShip->restMask, GPN_ALM_PAGE_NUM);
	
						break;
					}
					almSubTypeNum++;
					pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pAlmSubTpNode));
				}
				break;
			}
			almTypeNum++;
			pAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pAlmTpNode));
		}
		if(pAlmTpNode == NULL)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restraintemplate has undefined almType\n\r",\
				__FUNCTION__);
			gpnLog(GPN_LOG_L_ALERT, "%s : restraintemplate has undefined almType\n\r",\
				__FUNCTION__);
			return GPN_ALM_GEN_ERR;
		}
	}

	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmRestScanTypeInfoFinish(sysSupAlmRestTpTemp *pAlmRestTpTemp, formRestrainShip *pRestShipPool)
{
	formRestrainShip *pRestShip;
	stAlmScanTypeDef *pAlmScanType;
	stAlmSubTpNode *pAlmSubTpNode;
	stAlmSTCharacterDef *pAlmSubType;
	UINT32 almSubTpNumInRestTp;
	UINT32 almScanTypeNum;
	UINT32 almSubTypeNum;
	UINT32 i;

	/* first get almSubTpNumInRestTp */
	almSubTpNumInRestTp = 0;
	pRestShip = pRestShipPool;
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++,pRestShip++)
	{
		/* almRestRuleNum <= restShipPoolMax */
		if(pRestShip->almSubTp == 0)
		{
			break;
		}

		almSubTpNumInRestTp++;
	}

	/*printf("almRestType (%s) has %d subAlmType\n\r",\
		pAlmRestTpTemp->palmRestTpTemp->almRestTpName,\
		almSubTpNumInRestTp);
	printf(" almType bitP (231  203 202  174 173  145 144  116 115   87 86    58 57    29 28     0)\n\r");*/

	/* then finish scanType restrain info */
	pRestShip = pRestShipPool;
	for(i=0; i<pAlmRestTpTemp->almRestRuleNum; i++,pRestShip++)
	{
		/* almRestRuleNum <= restShipPoolMax */
		if(pRestShip->almSubTp == 0)
		{
			break;
		}
		
		/* finish scanType info */
		almScanTypeNum = 0;
		pAlmScanType = (stAlmScanTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almScanTypeQuen));
		while((pAlmScanType != NULL)&&(almScanTypeNum < pgstAlmTypeWholwInfo->almScanTypeNum))
		{
			/* if sanType already finished */
			if(pAlmScanType->almSubTpNumInRestType == 0)
			{
				pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmScanType->genAlmScanSubTCharaQuen));
				pAlmSubType = pAlmSubTpNode->pAlmSubTpStr;
				if(pAlmSubType != NULL)
				{
					if((pAlmSubType->almSubType & GPN_ALM_TYPE_MASK) == (pRestShip->almSubTp & GPN_ALM_TYPE_MASK))
					{	
						almSubTypeNum = 0;
						pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmScanType->genAlmScanSubTCharaQuen));
						while((pAlmSubTpNode != NULL)&&(almSubTypeNum < pAlmScanType->genAlmSubTpNumInScanType))
						{
							pAlmSubType = pAlmSubTpNode->pAlmSubTpStr;
							gpnAlmRestSpecialArraySetBit(pAlmScanType->scanTypeAlmMask,\
								GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);

							/* alarm template value */
							if(pAlmSubType->isScreen == DIS)
							{
								gpnAlmRestSpecialArraySetBit(pAlmScanType->almScreInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							else
							{
								gpnAlmRestSpecialArrayClearBit(pAlmScanType->almScreInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							
							if(pAlmSubType->isFilted == DIS)
							{
								gpnAlmRestSpecialArraySetBit(pAlmScanType->almFiltInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							else
							{
								gpnAlmRestSpecialArrayClearBit(pAlmScanType->almFiltInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							
							if(pAlmSubType->isReport == EN)
							{
								gpnAlmRestSpecialArraySetBit(pAlmScanType->almReptInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							else
							{
								gpnAlmRestSpecialArrayClearBit(pAlmScanType->almReptInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							
							if(pAlmSubType->isRecord == EN)
							{
								gpnAlmRestSpecialArraySetBit(pAlmScanType->almReckInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							else
							{
								gpnAlmRestSpecialArrayClearBit(pAlmScanType->almReckInfo,\
									GPN_ALM_PAGE_NUM, pAlmSubType->almUseBitPosition);
							}
							
							almSubTypeNum++;
							pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)pAlmSubTpNode);
						}

						/* scanType's restrain type's almSubTpNumInRestTp */
						pAlmScanType->almSubTpNumInRestType = almSubTpNumInRestTp;
						/* base restrain type for almUsePag, almUseBitPosition not continue*/
						pAlmScanType->almUsePag = pAlmScanType->almSubTpNumInRestType/GPN_ALM_PAGE_BIT_SIZE;
						if((pAlmScanType->almSubTpNumInRestType%GPN_ALM_PAGE_BIT_SIZE) != 0)
						{
							pAlmScanType->almUsePag++;
						}
						
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : almScanType %08x subTpNumInRestTp %d almUsePag %d\n\r",\
							__FUNCTION__, pAlmScanType->almScanType,\
							almSubTpNumInRestTp, pAlmScanType->almUsePag);
					}
				}
				else
				{
					GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : ScanTp(%08x) has not a AlmSubType\n\r",\
						__FUNCTION__, pAlmScanType->almScanType);
					gpnLog(GPN_LOG_L_ALERT, "%s : ScanTp(%08x) has not a AlmSubType\n\r",\
						__FUNCTION__, pAlmScanType->almScanType);
					return GPN_ALM_GEN_ERR;
				}

			}
			else
			{
				/* finished before */
			}

			almScanTypeNum++;
			pAlmScanType = (stAlmScanTypeDef *)listNext((NODE *)pAlmScanType);
		}

		/*printf("%08x %04d (%08x %08x %08x %08x %08x %08x %08x %08x)\n\r",\
			pRestShip->almSubTp, pRestShip->bitPosition,\
			pRestShip->restMask[7],pRestShip->restMask[6],\
			pRestShip->restMask[5],pRestShip->restMask[4],\
			pRestShip->restMask[3],pRestShip->restMask[2],\
			pRestShip->restMask[1],pRestShip->restMask[0]);*/
	}

	return GPN_ALM_GEN_OK;
}
