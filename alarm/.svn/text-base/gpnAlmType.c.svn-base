		(stAlmSTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmRestTypeNodePool +\
		almRestTypeNum*sizeof(stAlmRestTypeDef));
	
	pgstAlmTypeWholwInfo->pAlmTpIndexPoolforRestTpQ =\
		(stAlmTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmSTpIndexPoolforRestTpQ +\
		pgstAlmTypeWholwInfo->almScanTypeNum*sizeof(stAlmSTpNode));

	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforRestTpQ =\
		(stAlmSubTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmTpIndexPoolforRestTpQ +\
		pgstAlmTypeWholwInfo->almTypeNum*sizeof(stAlmSTpNode));

	pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenRestTpQ =\
		(stAlmTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforRestTpQ +\
		pgstAlmTypeWholwInfo->almSubTypeNum*sizeof(stAlmSubTpNode));

	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenRestTpQ =\
		(stAlmSubTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenRestTpQ +\
		pgstAlmTypeWholwInfo->genAlmTypeNum*sizeof(stAlmSTpNode));

	pgSysSupAlmRestTpTemp = gsysSupAlmRestTpTemp;
	almRestTypeNum = sizeof(gsysSupAlmRestTpTemp)/sizeof(gsysSupAlmRestTpTemp[0]);
	for(i=0,j=0,k=0,m=0,h=0,n=0;i<almRestTypeNum;i++,pgSysSupAlmRestTpTemp++)
	{
		/*find mem space */
		pAlmRestType = pgstAlmTypeWholwInfo->pAlmRestTypeNodePool + i;
		
		listAdd(&(pgstAlmTypeWholwInfo->almRestTypeQuen), (NODE *)pAlmRestType);
		pgstAlmTypeWholwInfo->almRestTypeNum++;

		/*init alarm rest type struct */
		pAlmRestTpTemp = pgSysSupAlmRestTpTemp->palmRestTpTemp;
		
		pAlmRestType->almRestType 				= pAlmRestTpTemp->almRestType;
		listInit(&(pAlmRestType->almRestScanTpQuen));
		listInit(&(pAlmRestType->almRestAlmTpQuen));
		listInit(&(pAlmRestType->almRestSubTCharaQuen));
		
		pAlmRestType->almScanTpNumInRestType 	= 0;	
		pAlmRestType->almTpNumInRestType	 	= 0;
		pAlmRestType->almSubTpNumInRestType 	= 0;

		listInit(&(pAlmRestType->genAlmRestAlmTpQuen));
		listInit(&(pAlmRestType->genAlmRestSubTCharaQuen));
		
		pAlmRestType->genAlmTpNumInRestType	 	= 0;
		pAlmRestType->genAlmSubTpNumInRestType 	= 0;
		
		pAlmRestType->almRestTypeName 			= pAlmRestTpTemp->almRestTpName;
		
		/*子类,告警类,扫描类告警属性到抑制类属性集中搬移*/
		for(l=0;l<pgSysSupAlmRestTpTemp->almScanNumInRestTp;l++,pAlmRestTpTemp++)
		{
			/*扫描类型加入到抑制类型中对应的队列*/
			almScanTypeNum = 0;
			pAlmScanType = (stAlmScanTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almScanTypeQuen));
			while((pAlmScanType != NULL)&&(almScanTypeNum<pgstAlmTypeWholwInfo->almScanTypeNum))
			{
				if(pAlmScanType->almScanType == pAlmRestTpTemp->almScanTpMemb)
				{	
					if(j < pgstAlmTypeWholwInfo->almScanTypeNum)
					{
						/*find mem space */
						pAlmSTpNode = pgstAlmTypeWholwInfo->pAlmSTpIndexPoolforRestTpQ + j;
						/*node index link */
						pAlmSTpNode->pAlmSTpStr = pAlmScanType;
						
						listAdd(&(pAlmRestType->almRestScanTpQuen),(NODE *)(pAlmSTpNode));
						pAlmRestType->almScanTpNumInRestType++;
						j++;
					}
					else
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almScanType into almRestType Quen err\n\r",\
							__FUNCTION__);
						gpnLog(GPN_LOG_L_ALERT, "%s : almScanType into almRestType Quen err\n\r",\
							__FUNCTION__);
						return GPN_ALM_GEN_ERR;
					}

					/*扫描类型中告警类型加入到抑制类型中对应的队列*/
					almTypeNum = 0;
					pAlmTpNode = (stAlmTpNode *)listFirst(&(pAlmScanType->almTpsOfScanTpQuen));
					while((pAlmTpNode != NULL)&&(almTypeNum<pAlmScanType->almTpNumInScanType))
					{
						if(k < pgstAlmTypeWholwInfo->almTypeNum)
						{
							/*find mem space */
							pAlmTpNodeExt = pgstAlmTypeWholwInfo->pAlmTpIndexPoolforRestTpQ + k;
							/*node index link */
							pAlmTpNodeExt->pAlmTpStr = pAlmTpNode->pAlmTpStr;
							
							listAdd(&(pAlmRestType->almRestAlmTpQuen),(NODE *)(pAlmTpNodeExt));
							pAlmRestType->almTpNumInRestType++;
							k++;
						}
						else
						{
							GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almType into almRestType Quen err\n\r",\
								__FUNCTION__);
							gpnLog(GPN_LOG_L_ALERT, "%s : almType into almRestType Quen err\n\r",\
								__FUNCTION__);
							return GPN_ALM_GEN_ERR;
						}
						
						almTypeNum++;
						pAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pAlmTpNode));
					}
					
					/*扫描类型中告警类型中告警子类加入到抑制类型中对应的队列*/
					almSubTypeNum = 0;
					pAlmSubType = (stAlmSTCharacterDef *)listFirst(&(pAlmScanType->almScanSubTCharaQuen));
					while((pAlmSubType != NULL)&&(almSubTypeNum<pAlmScanType->almSubTpNumInScanType))
					{
						if(m < pgstAlmTypeWholwInfo->almSubTypeNum)
						{
							/*find mem space */
							pAlmSubTpNode = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforRestTpQ + m;
							/*node index link */
							pAlmSubTpNode->pAlmSubTpStr = pAlmSubType;
							
							listAdd(&(pAlmRestType->almRestSubTCharaQuen),(NODE *)(pAlmSubTpNode));
							pAlmRestType->almSubTpNumInRestType++;
							m++;
						}
						else
						{
							GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almSubType into almRestType Quen err\n\r",\
								__FUNCTION__);
							gpnLog(GPN_LOG_L_ALERT, "%s : almSubType into almRestType Quen err\n\r",\
								__FUNCTION__);
							return GPN_ALM_GEN_ERR;
						}

						almSubTypeNum++;
						pAlmSubType = (stAlmSTCharacterDef *)listNext((NODE *)(pAlmSubType));
					}

					/* alarm num in rest type check */
					if(pAlmScanType->almSubTpNumInRestType > GPN_ALM_MAX_REST_ALM)
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almScanType(%08x) have too mutch subAlmType(%d)\n\r",\
							__FUNCTION__, pAlmScanType->almScanType,\
							pAlmScanType->almSubTpNumInRestType);
						gpnLog(GPN_LOG_L_ALERT, "%s : almScanType(%08x) have too mutch subAlmType(%d)\n\r",\
							__FUNCTION__, pAlmScanType->almScanType,\
							pAlmScanType->almSubTpNumInRestType);
						return GPN_EVT_GEN_ERR;
					}
					
					/*gen alarm type into alarm rest type */
					genAlmTypeNum = 0;
					pAlmTpNode = (stAlmTpNode *)listFirst(&(pAlmScanType->genAlmTpsOfScanTpQuen));
					while((pAlmTpNode != NULL)&&(genAlmTypeNum<pAlmScanType->genAlmTpNumInScanType))
					{
						if(h < pgstAlmTypeWholwInfo->genAlmTypeNum)
						{
							/*find mem space */
							pAlmTpNodeExt = pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenRestTpQ + h;
							/*node index link */
							pAlmTpNodeExt->pAlmTpStr = pAlmTpNode->pAlmTpStr;
							
							listAdd(&(pAlmRestType->genAlmRestAlmTpQuen),(NODE *)(pAlmTpNodeExt));
							pAlmRestType->genAlmTpNumInRestType++;
							h++;
						}
						else
						{
							GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : genAlmType into almRestType Quen err\n\r",\
								__FUNCTION__);
							gpnLog(GPN_LOG_L_ALERT, "%s : genAlmType into almRestType Quen err\n\r",\
								__FUNCTION__);
							return GPN_ALM_GEN_ERR;
						}
						
						genAlmTypeNum++;
						pAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pAlmTpNode));
					}

					/*gen sub alarm type into alarm rest type */
					genAlmSubTypeNum = 0;
					pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmScanType->genAlmScanSubTCharaQuen));
					while((pAlmSubTpNode != NULL)&&(genAlmSubTypeNum<pAlmScanType->genAlmSubTpNumInScanType))
					{
						if(n < pgstAlmTypeWholwInfo->genAlmSubTypeNum)
						{
							/*find mem space */
							pAlmSubTpNodeExt= pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenRestTpQ + n;
							/*node index link */
							pAlmSubTpNodeExt->pAlmSubTpStr = pAlmSubTpNode->pAlmSubTpStr;
							
							listAdd(&(pAlmRestType->genAlmRestSubTCharaQuen),(NODE *)(pAlmSubTpNodeExt));
							pAlmRestType->genAlmSubTpNumInRestType++;
							n++;
						}
						else
						{
							GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : genAlmSubType into almRestType Quen err\n\r",\
								__FUNCTION__);
							gpnLog(GPN_LOG_L_ALERT, "%s : genAlmSubType into almRestType Quen err\n\r",\
								__FUNCTION__);
							return GPN_ALM_GEN_ERR;
						}

						genAlmSubTypeNum++;
						pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pAlmSubTpNode));
					}
					
					break;
				}
				
				almScanTypeNum++;
				pAlmScanType = (stAlmScanTypeDef *)listNext((NODE *)(pAlmScanType));
			}
			if((pAlmScanType == NULL)&&(almScanTypeNum>=pgstAlmTypeWholwInfo->almScanTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almScanType into almRestType Quen err\n\r",\
					__FUNCTION__);
				gpnLog(GPN_LOG_L_ALERT, "%s : almScanType into almRestType Quen err\n\r",\
					__FUNCTION__);
				return GPN_ALM_GEN_ERR;
			}
		}
	}
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmAlmSubTypeSysIdentifyCreat(void)
{
	UINT32 page;
	UINT32 bitOrder;
	UINT32 sysId;
	UINT32 restTypeNum;
	stAlmRestTypeDef *pAlmRestType;
	UINT32 almSubTpNum;
	stAlmSubTpNode *pAlmSubTpNode;
	
	/* base AlmRestType */
	restTypeNum = 0;
	pAlmRestType = (stAlmRestTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almRestTypeQuen));
	while((pAlmRestType != NULL)&&(restTypeNum<pgstAlmTypeWholwInfo->almRestTypeNum))
	{
		almSubTpNum = 0;
		pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmRestType->genAlmRestSubTCharaQuen));
		while((pAlmSubTpNode != NULL)&&(almSubTpNum<pAlmRestType->genAlmSubTpNumInRestType))
		{
			/* creat alm sub type sys identify */
			page = almSubTpNum / GPN_ALM_PAGE_BIT_SIZE;
			bitOrder = almSubTpNum % GPN_ALM_PAGE_BIT_SIZE;
			sysId = (page << GPN_ALM_PAGE_BIT_SIZE) | (1U << bitOrder);

			pAlmSubTpNode->pAlmSubTpStr->sysUseIdentify = sysId;

			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : in restType(%08x) almSubType(%08x) sysUseIdentify (%08x)\n\r",\
				__FUNCTION__, pAlmRestType->almRestType,\
				pAlmSubTpNode->pAlmSubTpStr->almSubType,\
				pAlmSubTpNode->pAlmSubTpStr->sysUseIdentify);
			/*printf("%s : in restType(%08x) almSubType(%08x) sysUseIdentify (%08x)\n\r",\
				__FUNCTION__, pAlmRestType->almRestType,\
				pAlmSubTpNode->pAlmSubTpStr->almSubType,\
				pAlmSubTpNode->pAlmSubTpStr->sysUseIdentify);*/
			
			almSubTpNum++;
			pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)pAlmSubTpNode);
		}
		
		restTypeNum++;
		pAlmRestType = (stAlmRestTypeDef *)listNext((NODE *)pAlmRestType);
	}
	
	return GPN_ALM_GEN_OK;
}

UINT32 gpnAlmStructAlmRestRuleCheck(void)
{
	UINT32 i;
	UINT32 j;
	UINT32 restType;
	UINT32 almRestTypeNum;
	UINT32 restTypeNum;
	UINT32 subTypeNum;

	stAlmSubTpNode *pAlmSubTpNode;
	stAlmRestTypeDef *pAlmRestType;
	sysSupAlmRestTpTemp *pgSysSupAlmRestTpTemp;
	
	pgSysSupAlmRestTpTemp = gsysSupAlmRestTpTemp;
	almRestTypeNum = sizeof(gsysSupAlmRestTpTemp)/sizeof(gsysSupAlmRestTpTemp[0]);
	for(i=0;i<almRestTypeNum;i++,pgSysSupAlmRestTpTemp++)
	{
		restType = pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestType;
		/* find almRestType in pgstAlmTypeWholwInfo */
		restTypeNum = 0;
		pAlmRestType = (stAlmRestTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almRestTypeQuen));
		while((pAlmRestType != NULL)&&(restTypeNum<pgstAlmTypeWholwInfo->almRestTypeNum))
		{
			if(pAlmRestType->almRestType == restType)
			{
				/*check if alarm sub type all in rest type as 'beResAlmTp' */
				subTypeNum = 0;
				pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmRestType->genAlmRestSubTCharaQuen));
				while((pAlmSubTpNode != NULL)&&(subTypeNum<pAlmRestType->genAlmSubTpNumInRestType))
				{
					/* find almSubType in SingAndMulResTemp */
					for(j=0;j<pgSysSupAlmRestTpTemp->almRestRuleNum;j++)
					{
						if(pgSysSupAlmRestTpTemp->SingAndMulResTemp[j].beResAlmTp ==\
							pAlmSubTpNode->pAlmSubTpStr->almSubType)
						{
							GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : in restType(%08x) line(%d), almSubType(%08x) rest by (%08x)\n\r",\
								__FUNCTION__, restType, j+1,\
								pgSysSupAlmRestTpTemp->SingAndMulResTemp[j].beResAlmTp,\
								pgSysSupAlmRestTpTemp->SingAndMulResTemp[j].whoResMe);
							break;
						}
					}

					if(j>=pgSysSupAlmRestTpTemp->almRestRuleNum)
					{
						/* can not find almSubType, this result in almSubType has no almSysBitp */
						fprintf(stderr, "%s : in restType(%08x), almSubType(%08x) not be rest\n\r",\
							__FUNCTION__, restType, pAlmSubTpNode->pAlmSubTpStr->almSubType);
					}

					subTypeNum++;
					pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pAlmSubTpNode));
				}
				
				break;
			}
			
			restTypeNum++;
			pAlmRestType = (stAlmRestTypeDef *)listNext((NODE *)(pAlmRestType));
		}
		if((pAlmRestType == NULL)||(restTypeNum>=pgstAlmTypeWholwInfo->almRestTypeNum))
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%08x) find in almRestTypeQuen Quen err\n\r",\
				__FUNCTION__, restType);
			gpnLog(GPN_LOG_L_ALERT, "%s : restType(%08x) find in almRestTypeQuen Quen err\n\r",\
				__FUNCTION__, restType);
			return GPN_ALM_GEN_ERR;
		}
		
	}
	
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmStructAlmRestRuleInit(void)
{
	UINT32 i;
	UINT32 almRestTypeNum;
	formRestrainShip *pRestShipPoolHead;
	sysSupAlmRestTpTemp *pgSysSupAlmRestTpTemp;	
	pgSysSupAlmRestTpTemp = gsysSupAlmRestTpTemp;
	almRestTypeNum = sizeof(gsysSupAlmRestTpTemp)/sizeof(gsysSupAlmRestTpTemp[0]);
	for(i=0;i<almRestTypeNum;i++,pgSysSupAlmRestTpTemp++)
	{
		/* init for restShipPool */
		pRestShipPoolHead = NULL;
		if(gpnAlmpRestShipPoolInit(pgSysSupAlmRestTpTemp, &pRestShipPoolHead) != GPN_ALM_GEN_OK)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%s) pool init error\n\r",\
				__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
			gpnLog(GPN_LOG_L_ALERT, "%s : restType(%s) pool init error\n\r",\
				__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
			
			return GPN_ALM_GEN_ERR;
		}

		/* read restTemp ruler in restPool */
		if(gpnAlmpRestRulerReadInRestShipPool(pgSysSupAlmRestTpTemp, pRestShipPoolHead) != GPN_ALM_GEN_OK)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%s) rest ruler read error\n\r",\
				__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
			gpnLog(GPN_LOG_L_ALERT, "%s : restType(%s) rest ruler read error\n\r",\
				__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
			
			return GPN_ALM_GEN_ERR;
		}

		/*test restShipPool after rest ruler read in */
		//debugGpnAlmRestShipPool(pgSysSupAlmRestTpTemp, pRestShipPoolHead);

		if(pgSysSupAlmRestTpTemp->SingAndMulResTemp->SingOrMulRes == 1)
		{
			/* reorder base level */
			if(gpnAlmpRestShipPoolReorderBaseLevel(pgSysSupAlmRestTpTemp, pRestShipPoolHead) != GPN_ALM_GEN_OK)
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%s) reorder error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				gpnLog(GPN_LOG_L_ALERT, "%s : restType(%s) reorder error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				
				return GPN_ALM_GEN_ERR;
			}

			/* reorder base bitPosition */
			if(gpnAlmpRestShipPoolReorderBaseBitPosition(pgSysSupAlmRestTpTemp, pRestShipPoolHead) != GPN_ALM_GEN_OK)
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%s) reorder error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				gpnLog(GPN_LOG_L_ALERT, "%s : restType(%s) reorder error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				
				return GPN_ALM_GEN_ERR;
			}

			/* restMask transmit, start from low level */
			if(gpnAlmpRestShipPoolRestTransmit(pgSysSupAlmRestTpTemp, pRestShipPoolHead) != GPN_ALM_GEN_OK)
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restType(%s) rest transmit error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				gpnLog(GPN_LOG_L_ALERT, "%s : restType(%s) rest transmit error\n\r",\
					__FUNCTION__, pgSysSupAlmRestTpTemp->palmRestTpTemp->almRestTpName);
				
				return GPN_ALM_GEN_ERR;
			}
			
		}
		else if(pgSysSupAlmRestTpTemp->SingAndMulResTemp->SingOrMulRes == 2)
		{
			/* rest between restType now not support */
		}
		else
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restrain template err 2\n\r",\
				__FUNCTION__);
			gpnLog(GPN_LOG_L_ALERT, "%s : restrain template err 2\n\r",\
				__FUNCTION__);
			return GPN_ALM_GEN_ERR;
		}
		
		/* finish subType restrain info */
		if(GPN_ALM_GEN_OK != gpnAlmRestSubTypeInfoFinish(pgSysSupAlmRestTpTemp, pRestShipPoolHead))
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : finish almSubType info error!\n\r",\
				__FUNCTION__);
			gpnLog(GPN_LOG_L_ALERT, "%s : finish almSubType info error!\n\r",\
				__FUNCTION__);
			return GPN_ALM_GEN_ERR;
		}

		/* finish scanType restrain info */
		if(GPN_ALM_GEN_OK != gpnAlmRestScanTypeInfoFinish(pgSysSupAlmRestTpTemp, pRestShipPoolHead))
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : finish almScanType info error!\n\r",\
				__FUNCTION__);
			gpnLog(GPN_LOG_L_ALERT, "%s : finish almScanType info error!\n\r",\
				__FUNCTION__);
			return GPN_ALM_GEN_ERR;
		}
		
		//debugGpnAlmRestShipPool(pgSysSupAlmRestTpTemp, pRestShipPoolHead);

		/* restShipPool free */
		if(pRestShipPoolHead != NULL)
		{
			free((void *)pRestShipPoolHead);
		}
	}
	
	return GPN_ALM_GEN_YES;
}
UINT32 gpnAlmpRestShipPoolInit(sysSupAlmRestTpTemp *pgAlmRestTpTemp, formRestrainShip **ppRestShipPoolHead)
{
	formRestrainShip *pRestShipPool;
	UINT32 poolSize;
	
	/* assert */
	if( (pgAlmRestTpTemp == NULL) ||\
		(ppRestShipPoolHead == NULL) )
	{
		return GPN_ALM_GEN_ERR;
	}

	/* malloc for mem space : +1 is must, for pointer like pool pool+1 */
	poolSize = (pgAlmRestTpTemp->almRestRuleNum + 1)*sizeof(formRestrainShip);
	pRestShipPool = (formRestrainShip *)malloc(poolSize);
	if(pRestShipPool == NULL)
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : restrain rule memalloc err\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : restrain rule memalloc err\n\r",\
			__FUNCTION__);
		
		return GPN_ALM_GEN_ERR;
	}

	/* memset 0 */
	memset((UINT8 *)pRestShipPool, 0, poolSize);
	*ppRestShipPoolHead = pRestShipPool;
	
	/**/gpnLog(GPN_LOG_L_ALERT, "%s : restTypd(%s) restRuleNum(%d) poolSize(%d)\n\r",\
		__FUNCTION__, pgAlmRestTpTemp->palmRestTpTemp->almRestTpName,\
		pgAlmRestTpTemp->almRestRuleNum, poolSize);

	return GPN_ALM_GEN_OK;
}
