			}
			
			//debugGpnAlmRestShipPool(pgAlmRestTpTemp, pRestShipPoolHead);

			/* ####  canRest find */
			if(gpnAlmpRestRulerCanRestAlmReadIn(pgAlmRestTpTemp, pRestrainTemp,
				pRestShipPoolHead, pRestShipBeRest, &topLevelNow, &topLevelMach) != GPN_ALM_GEN_OK)
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP,\
					"%s : ruler(%d) canRest(%08x) beRest (%08x) read err\n\r",\
					__FUNCTION__, pRestrainTemp->indexNum,\
					pRestrainTemp->whoResMe, pRestrainTemp->beResAlmTp);
				gpnLog(GPN_LOG_L_ALERT,\
					"%s : ruler(%d) canRest(%08x) beRest (%08x) read err\n\r",\
					__FUNCTION__, pRestrainTemp->indexNum,\
					pRestrainTemp->whoResMe, pRestrainTemp->beResAlmTp);
				
				return GPN_ALM_GEN_ERR;
			}
			
			//debugGpnAlmRestShipPool(pgAlmRestTpTemp, pRestShipPoolHead);

		}				
