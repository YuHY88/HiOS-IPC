void gpnAlmCodeShow (struct vty *vty)
{
	int alarm_num = sizeof(gstAlmTypeTemplate)/sizeof(gstAlmTypeTemplate[0]);
	stAlmRTCharacterDef *pgstAlmTypeTemp = gstAlmTypeTemplate;
	int i = 0;
	if (alarm_num)
	{
		vty_out (vty, "AlmCode  Type        DefLevel Shield Report Filted Note %s", VTY_NEWLINE);
		vty_out (vty, "-------- ----------- -------- ------ ------ ------ -----------------------%s", VTY_NEWLINE);
		for (i = 0; i < alarm_num; i++, pgstAlmTypeTemp++)
		{
			if (IS_GPN_ALM_SUB_TYPE(pgstAlmTypeTemp->almSubType))
				vty_out (vty, "%08x %-11s %-8s %-6s %-6s %-6s %s %s",
					pgstAlmTypeTemp->almSubType,
					gpnAlmFuncStrGet (pgstAlmTypeTemp->almFunc),
					gpnAlmLevelStrGet (pgstAlmTypeTemp->almRank),
					pgstAlmTypeTemp->isScreen ? "true" : "false",
					pgstAlmTypeTemp->isReport ? "true" : "false",
					pgstAlmTypeTemp->isFilted ? "true" : "false",
					pgstAlmTypeTemp->almName,
					VTY_NEWLINE);
			else
				continue;
		}
		vty_out (vty, "-------- ----------- -------- ------ ------ ------ -----------------------%s", VTY_NEWLINE);
		vty_out (vty, "total number : %d %s", alarm_num, VTY_NEWLINE);
		vty_out (vty, "--------------------------------------------------------------------------%s", VTY_NEWLINE);
	}	
}

