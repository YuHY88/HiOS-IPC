	UINT32 i;
	UINT32 pageBit;
	UINT32 upBit;
	UINT32 temp;
	
	if((array == NULL)||(scale >GPN_ALM_PAGE_BIT_SIZE)||(arraySize > GPN_ALM_PAGE_NUM))
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : LeftMov severity error\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : LeftMov severity error\n\r",\
			__FUNCTION__);
		return GPN_ALM_GEN_NO;
	}
	else
	{
		for(i=0; i<arraySize; i++)
		{
			/*save page info*/
			pageBit = array[i] & GPN_ALM_PAGE_MASK;
			/*get up(left over) bit info*/
			temp = (1U << scale) - 1;
			temp <<= (GPN_ALM_PAGE_BIT_SIZE - scale);
			upBit = ((array[i] & temp) >> (GPN_ALM_PAGE_BIT_SIZE - scale));

			/*bit mov*/
			array[i] <<= scale;
			/*add page info*/
			array[i] = (array[i] & GPN_ALM_IN_PAGE_BITP) | pageBit;
			/*add last up(left over) bit info*/
			array[i] |= (fillBit & ((1U << scale) - 1));

			/*save this up(lefe over) bit info*/
			fillBit = upBit;

			/*printf("%08x %08x %08x %08x\n\r",array[3],array[2],array[1],array[0]);*/
			/*printf("page %08x upbit %08x fillbit %08x\n\r",pageBit,upBit,fillBit);*/
		}
		
		return GPN_ALM_GEN_YES;
	}
}
