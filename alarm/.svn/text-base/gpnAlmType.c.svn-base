	genEvtSubTypeNum = sizeof(gstEvtTypeTemplate)/sizeof(gstEvtTypeTemplate[0]);

	genEvtTypeNum = 0;
	for(i=0,j=0;i<genEvtSubTypeNum;i++,pgstEvtTypeTemp++)
	{
		if(IS_GPN_EVT_SUB_TYPE(pgstEvtTypeTemp->evtSubType))
		{
			j++;
		}
		else
		{
			genEvtTypeNum++;
		}
	}
	
	if( ((genEvtTypeNum+j) == genEvtSubTypeNum) &&
		/* event type corresponding to alarm type */
		(genEvtTypeNum <= pgstAlmTypeWholwInfo->almTypeNum) )
	{
		genEvtSubTypeNum = j;
	}
	else
	{
		GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s : evtSubTpTemp & evtTpTemp err\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : evtSubTpTemp & evtTpTemp err\n\r",\
			__FUNCTION__);
		return GPN_EVT_GEN_ERR;
	}

	/***************************************************************************/
	/*                                                    mem view                                                                 */
	/***************************************************************************/
	/* 1  event type info struct                            stEvtTypeDef                    genEvtTypeNum       */
	/* 2  sub event type info struct                      stEvtSTCharacterDef          genEvtSubTypeNum */
	/* 3  evt type node for gen evt type               stEvtTpNode                     genEvtTypeNum       */
	/* 4  sub evt type node for gen sub evt type   stEvtSubTpNode                genEvtSubTypeNum */
	/* 5  sub evt type node for gen evt type         stEvtSubTpNode                genEvtSubTypeNum */
	/****************************************************************************/
	i = genEvtTypeNum*sizeof(stEvtTypeDef) +/*gen event types info size*/\
		genEvtSubTypeNum*sizeof(stEvtSTCharacterDef) +/*gen event sub types info size*/\
		genEvtTypeNum*sizeof(stEvtTpNode) +/*event type node to gen event type quen*/\
		genEvtSubTypeNum*sizeof(stEvtSubTpNode) +/*event sub type node to gen event type quen*/\
		genEvtSubTypeNum*sizeof(stEvtSubTpNode)/*event sub type node to gen event sub type quen*/;

	evtMemSpace = (UINT8*)malloc(i);
	if(evtMemSpace == NULL)
	{
		perror("evtMemSpace malloc err:");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_EVT_GEN_ERR;
	}
	memset((UINT8 *)evtMemSpace, 0, i);

	pgstAlmTypeWholwInfo->pEvtTypeNodePool =\
		(stEvtTypeDef *)((UINT8*)(evtMemSpace));
	
	pgstAlmTypeWholwInfo->pEvtSubTypeNodePool =\
		(stEvtSTCharacterDef*)((UINT8*)(pgstAlmTypeWholwInfo->pEvtTypeNodePool) +\
		genEvtTypeNum*sizeof(stEvtTypeDef));

	pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenEvtTpQ =\
		(stEvtTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pEvtSubTypeNodePool) +\
		genEvtSubTypeNum*sizeof(stEvtSTCharacterDef));
		
	pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenSubTpQ =\
		(stEvtSubTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenEvtTpQ) +\
		genEvtTypeNum*sizeof(stEvtTpNode));
	
	pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenEvtTpQ =\
		(stEvtSubTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenSubTpQ) +\
		genEvtSubTypeNum*sizeof(stEvtSubTpNode));

	listInit(&(pgstAlmTypeWholwInfo->genEvtTypeQuen));	
	listInit(&(pgstAlmTypeWholwInfo->genEvtSubTypeQuen));
	pgstAlmTypeWholwInfo->genEvtTypeNum = 0;
	pgstAlmTypeWholwInfo->genEvtSubTypeNum = 0;

	listInit(&(pgstAlmTypeWholwInfo->evtTypeQuen));	
	listInit(&(pgstAlmTypeWholwInfo->evtSubTypeQuen));
	pgstAlmTypeWholwInfo->evtTypeNum = 0;
	pgstAlmTypeWholwInfo->evtSubTypeNum = 0;

	pgstEvtTypeTemp = gstEvtTypeTemplate;
	genEvtSubTypeNum = sizeof(gstEvtTypeTemplate)/sizeof(gstEvtTypeTemplate[0]);/*表有多大*/
	for(i=0,j=0,k=0;i<genEvtSubTypeNum;i++,pgstEvtTypeTemp++)
	{

		if(IS_GPN_EVT_SUB_TYPE(pgstEvtTypeTemp->evtSubType))
		{
			/*init subEvtType info */
			pEvtSubType = pgstAlmTypeWholwInfo->pEvtSubTypeNodePool + j;
			pEvtSubType->evtSubType        = pgstEvtTypeTemp->evtSubType;
			pEvtSubType->evtFunc           = pgstEvtTypeTemp->evtFunc;
			pEvtSubType->evtRank 		   = pgstEvtTypeTemp->evtRank;
			pEvtSubType->isScreen 		   = pgstEvtTypeTemp->isScreen;
			pEvtSubType->isReport 		   = pgstEvtTypeTemp->isReport;
			pEvtSubType->detial 		   = pgstEvtTypeTemp->detial;
			pEvtSubType->evtUseBitPosition = 0;
			pEvtSubType->sysUseIdentify	   = 0;
			pEvtSubType->evtName		   = pgstEvtTypeTemp->evtName;
			
			/*add evtSubType to gen evt sub type quen */
			pEvtSubTpNode = pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenSubTpQ + j;
			pEvtSubTpNode->pEvtSubTpStr = pEvtSubType;
			listAdd(&(pgstAlmTypeWholwInfo->genEvtSubTypeQuen), (NODE *)pEvtSubTpNode);
			pgstAlmTypeWholwInfo->genEvtSubTypeNum++;

			/*add evtSubType to gen evt type quen */
			pEvtSubTpNode = pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenEvtTpQ + j;
			pEvtSubTpNode->pEvtSubTpStr = pEvtSubType;

			genEvtTypeNum = 0;
			pEvtTpNode = (stEvtTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genEvtTypeQuen));
			while((pEvtTpNode != NULL)&&(genEvtTypeNum < pgstAlmTypeWholwInfo->genEvtTypeNum))
			{
				if(pEvtTpNode->pEvtTpStr->evtType ==\
					(pEvtSubTpNode->pEvtSubTpStr->evtSubType & GPN_EVT_TYPE_MASK))
				{
					listAdd(&(pEvtTpNode->pEvtTpStr->genEvtSubTypeCharaQuen), (NODE *)pEvtSubTpNode);
					pEvtTpNode->pEvtTpStr->genEvtNumInType++;
					break;
				}
				genEvtTypeNum++;
				pEvtTpNode = (stEvtTpNode *)listNext((NODE *)(pEvtTpNode));
			}
			if(pEvtTpNode == NULL)
			{
				GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s : evtSubType(%08x) to evtType err\n\r",
					__FUNCTION__, pEvtSubTpNode->pEvtSubTpStr->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : evtSubType(%08x) to evtType err\n\r",
					__FUNCTION__, pEvtSubTpNode->pEvtSubTpStr->evtSubType);
				return GPN_EVT_GEN_ERR;
			}
			
			j++;
		}
		else
		{
			/*find mem space */
			pEvtType = pgstAlmTypeWholwInfo->pEvtTypeNodePool + k;
			pEvtTpNode = pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenEvtTpQ + k;
			/*node index link */
			pEvtTpNode->pEvtTpStr = pEvtType;

			/*init gen event type node info */
			pEvtType->evtType		= (pgstEvtTypeTemp->evtSubType & GPN_EVT_TYPE_MASK);
			pEvtType->genEvtNumInType = 0;
			listInit(&(pEvtType->genEvtSubTypeCharaQuen));
			pEvtType->evtTypeName	= pgstEvtTypeTemp->evtName;
			
			listAdd(&(pgstAlmTypeWholwInfo->genEvtTypeQuen), (NODE *)pEvtTpNode);
			pgstAlmTypeWholwInfo->genEvtTypeNum++;
			k++;
		}
	}

	return GPN_EVT_GEN_OK;
}

UINT32 gpnEvtStructDevBaseEvtTypeInit(void)
{
	UINT8* evtMemSpace;
	
	UINT32 i;
	UINT32 j;
	UINT32 k;
	UINT32 evtTypeNum;
	UINT32 evtSubTypeNum;
	UINT32 genEvtTypeNum;
	UINT32 genEvtSubTypeNum;
	stEvtSubTypeBaseDev *pgstEvtSubTypeBaseDevTemp;

	stEvtSubTpNode *pGenEvtSubTpNode;
	stEvtSubTpNode *pEvtSubTpNode;
	stEvtTpNode *pGenEvtTpNode;
	stEvtTypeDef *pEvtType;
	UINT32 devType;

	gpnDevApiGetDevType(&devType, NULL, 0);
	
	/*gEvtSubTypeBaseDevTemplate table check */
	pgstEvtSubTypeBaseDevTemp = gEvtSubTypeBaseDevTemplate;
	/*get table line */
	evtSubTypeNum = sizeof(gEvtSubTypeBaseDevTemplate)/sizeof(gEvtSubTypeBaseDevTemplate[0]);

	evtTypeNum = 0;
	for(i=0,j=0;i<evtSubTypeNum;i++,pgstEvtSubTypeBaseDevTemp++)
	{
		if(IS_GPN_ALM_SUB_TYPE(pgstEvtSubTypeBaseDevTemp->evtSubType))
		{
			j++;
		}
		else
		{
			evtTypeNum++;
		}
	}

	if((evtTypeNum+j) == evtSubTypeNum)
	{
		evtSubTypeNum = j;
	}
	else
	{
		GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s : evtTypeNum(%d) evtSubTypeNum(%d), gEvtSubTypeBaseDevTemplate err!\n\r",\
			__FUNCTION__, evtTypeNum, evtSubTypeNum);
		gpnLog(GPN_LOG_L_ALERT, "%s : evtTypeNum(%d) evtSubTypeNum(%d), gEvtSubTypeBaseDevTemplate err!\n\r",\
			__FUNCTION__, evtTypeNum, evtSubTypeNum);
		return GPN_ALM_GEN_ERR;
	}

	/***************************************************************************/
	/*                                                    mem view                                                                 */
	/***************************************************************************/
	/* 1  sub event type node for devbase sub event type  stEvtSubTpNode      evtSubTypeNum   */
	/* 2  sub event type node for devbase event type        stEvtSubTpNode      evtSubTypeNum   */
	/****************************************************************************/
	i = 2*evtSubTypeNum*sizeof(stAlmSubTpNode)/*sub type to devbase sub event type & devbase event type*/;

	evtMemSpace = (UINT8*)malloc(i);
	if(evtMemSpace == NULL)
	{
		perror("evtMemSpace malloc : ");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_ALM_GEN_ERR;
	}
	memset((UINT8*)evtMemSpace, 0, i);

	pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforSubTpQ =\
		(stEvtSubTpNode *)((UINT8*)(evtMemSpace));
	
	pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforEvtTpQ =\
		(stEvtSubTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforSubTpQ) +\
		evtSubTypeNum*sizeof(stEvtSubTpNode));

	/*gEvtSubTypeBaseDevTemplate table check */
	pgstEvtSubTypeBaseDevTemp = gEvtSubTypeBaseDevTemplate;
	/*get table line */
	evtSubTypeNum = sizeof(gEvtSubTypeBaseDevTemplate)/sizeof(gEvtSubTypeBaseDevTemplate[0]);
	for(i=0,j=0,k=0;i<evtSubTypeNum;i++,pgstEvtSubTypeBaseDevTemp++)
	{
		if(pgstEvtSubTypeBaseDevTemp->devSup.support[devType] == DIS)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : dev(%d) do not support evtType(%08x)\n\r",\
				__FUNCTION__, devType, pgstEvtSubTypeBaseDevTemp->evtSubType);
			continue;
		}

		if(IS_GPN_EVT_SUB_TYPE(pgstEvtSubTypeBaseDevTemp->evtSubType))
		{
			/*check 1 : check event type in devSubEvebt type quen, if not , err happen or alarm type not sup */
			evtTypeNum = 0;
			pEvtType = (stEvtTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->evtTypeQuen));
			while((pEvtType != NULL)&&(evtTypeNum < pgstAlmTypeWholwInfo->evtTypeNum))
			{
				if(pEvtType->evtType == (pgstEvtSubTypeBaseDevTemp->evtSubType & GPN_EVT_TYPE_MASK))
				{
					/*find sub event type in devSubEvent type Quen*/
					break;
				}
				evtTypeNum++;
				pEvtType = (stEvtTypeDef *)listNext((NODE *)(pEvtType));
			}
			if((pEvtType == NULL)||(evtTypeNum >= pgstAlmTypeWholwInfo->evtTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : find evtSubType(%08x) in devEvtSubTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : find evtSubType(%08x) in devEvtSubTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				continue;
			}
			
			/*check 2 : check event type in genSubEvebt type quen, if not , err happen */
			genEvtSubTypeNum = 0;
			pGenEvtSubTpNode = (stEvtSubTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genEvtSubTypeQuen));
			while((pGenEvtSubTpNode != NULL)&&(genEvtSubTypeNum < pgstAlmTypeWholwInfo->genEvtSubTypeNum))
			{
				if(pGenEvtSubTpNode->pEvtSubTpStr->evtSubType == pgstEvtSubTypeBaseDevTemp->evtSubType)
				{
					/*find sub event type in genSubEvent type Quen*/
					break;
				}
				genEvtSubTypeNum++;
				pGenEvtSubTpNode = (stEvtSubTpNode *)listNext((NODE *)(pGenEvtSubTpNode));
			}
			if((pGenEvtSubTpNode == NULL)||(genEvtSubTypeNum >= pgstAlmTypeWholwInfo->genEvtSubTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : find evtSubType(%08x) in genEvtSubTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : find evtSubType(%08x) in genEvtSubTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			/*find mem space */
			pEvtSubTpNode = pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforSubTpQ + j;
			/*node index link */
			pEvtSubTpNode->pEvtSubTpStr = pGenEvtSubTpNode->pEvtSubTpStr;

			listAdd(&(pgstAlmTypeWholwInfo->evtSubTypeQuen), (NODE *)pEvtSubTpNode);
			pgstAlmTypeWholwInfo->evtSubTypeNum++;

			/*find mem space */
			pEvtSubTpNode = pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforEvtTpQ + j;
			/*node index link */
			pEvtSubTpNode->pEvtSubTpStr = pGenEvtSubTpNode->pEvtSubTpStr;
			
			evtTypeNum = 0;
			pEvtType = (stEvtTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->evtTypeQuen));
			while((pEvtType != NULL)&&(evtTypeNum < pgstAlmTypeWholwInfo->evtTypeNum))
			{
				if(pEvtType->evtType == (pgstEvtSubTypeBaseDevTemp->evtSubType & GPN_EVT_TYPE_MASK))
				{
					listAdd(&(pEvtType->evtSubTypeCharaQuen), (NODE *)pEvtSubTpNode);
					pEvtType->evtNumInType++;
					break;
				}
				evtTypeNum++;
				pEvtType = (stEvtTypeDef *)listNext((NODE *)(pEvtType));
			}
			if((pEvtType == NULL)||(evtTypeNum >= pgstAlmTypeWholwInfo->evtTypeNum))
			{
				GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s : evtSubType(%08x) to evtType err\n\r",\
					__FUNCTION__, pEvtSubTpNode->pEvtSubTpStr->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : evtSubType(%08x) to evtType err\n\r",\
					__FUNCTION__, pEvtSubTpNode->pEvtSubTpStr->evtSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			j++;
		}
		else
		{
			/*check alarm type in genAlarm type quen*/
			genEvtTypeNum = 0;
			pGenEvtTpNode = (stEvtTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genEvtTypeQuen));
			while((pGenEvtTpNode != NULL)&&(genEvtTypeNum < pgstAlmTypeWholwInfo->genEvtTypeNum))
			{
				if(pGenEvtTpNode->pEvtTpStr->evtType == (pgstEvtSubTypeBaseDevTemp->evtSubType & GPN_EVT_TYPE_MASK))
				{
					/*find event type in genEvent type Quen*/
					break;
				}
				genEvtTypeNum++;
				pGenEvtTpNode = (stEvtTpNode *)listNext((NODE *)(pGenEvtTpNode));
			}
			if((pGenEvtTpNode == NULL)||(genEvtTypeNum >= pgstAlmTypeWholwInfo->genEvtTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : find evtType(%08x) in genEvtTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : find evtType(%08x) in genEvtTypeQuen err\n\r",\
					__FUNCTION__, pgstEvtSubTypeBaseDevTemp->evtSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			listAdd(&(pgstAlmTypeWholwInfo->evtTypeQuen), (NODE *)(pGenEvtTpNode->pEvtTpStr));
			pgstAlmTypeWholwInfo->evtTypeNum++;
			
			k++;
		}
	}

	return GPN_EVT_GEN_OK;
}

UINT32 gpnEvtStruct2ScanTypeInit(void)
{
	UINT8* evtMemSpace;
	
	UINT32 i;
	UINT32 j;
	UINT32 k;
	UINT32 l;
	UINT32 genAlmTypeNum;
	UINT32 almScanTypeNum;
	UINT32 evtTypeNum;
	UINT32 evtSubTypeNum;
	UINT32 genEvtTypeNum;
	UINT32 genEvtSubTypeNum;

	stEvtTypeDef *pEvtType;
	stEvtTpNode *pEvtTpNode;
	stEvtSubTpNode *pEvtSubTpNode;
	
	stEvtTpNode *pgenEvtTpNode;
	stEvtTpNode *pgenEvtTpNodeTmp;
	stEvtSubTpNode *pgenEvtSubTpNode;
	stEvtSubTpNode *pgenEvtSubTpNodeTmp;
	
	stAlmTpNode *pGenAlmTpNode;
	stAlmScanTypeDef *pAlmScanType;

	/*********************************************************************************/
	/*                                                    mem view                                                                           */
	/*********************************************************************************/
	/* 1 dev event type node for almScanType's dev eventType Quen  stEvtTpNode         evtTypeNum      */
	/* 2 gen event type node for almScanType's gen eventType Quen  stEvtTpNode         gen evtTypeNum */
	/* 3 gen sub event type node for almScanType's gen eventType Quen  stEvtTpNode   gen sub evtTp Num*/
	/*********************************************************************************/
	i = pgstAlmTypeWholwInfo->evtTypeNum*sizeof(stEvtTpNode) +\
		pgstAlmTypeWholwInfo->genEvtTypeNum*sizeof(stEvtTpNode) +\
		pgstAlmTypeWholwInfo->genEvtSubTypeNum*sizeof(stEvtSubTpNode);
	evtMemSpace = (UINT8*)malloc(i);
	if(evtMemSpace == NULL)
	{
		perror("evtMemSpace malloc : ");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_ALM_GEN_ERR;
	}
	memset((UINT8*)evtMemSpace, 0, i);

	pgstAlmTypeWholwInfo->pEvtTpIndexPoolforScanTpQ =\
		(stEvtTpNode *)((UINT8*)(evtMemSpace));
	
	pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenScanTpQ =\
		(stEvtTpNode *)((UINT8*)(pgstAlmTypeWholwInfo->pEvtTpIndexPoolforScanTpQ) +\
		pgstAlmTypeWholwInfo->evtTypeNum*sizeof(stEvtTpNode));
	
	pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenScanTpQ =\
		(stEvtSubTpNode *)((UINT8*)(pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenScanTpQ) +\
		pgstAlmTypeWholwInfo->genEvtTypeNum*sizeof(stEvtTpNode));
	
	/*just dev base evevt type to alarm scan type*/
	i = 0;
	j = 0;
	k = 0;
	almScanTypeNum = 0;
	pAlmScanType = (stAlmScanTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almScanTypeQuen));
	while((pAlmScanType != NULL) && (almScanTypeNum < pgstAlmTypeWholwInfo->almScanTypeNum))
	{
		/*event list init*/
		listInit(&(pAlmScanType->genEvtScanSubTCharaQuen));	
		listInit(&(pAlmScanType->genEvtTpsOfScanTpQuen));
		pAlmScanType->genEvtSubTpNumInScanType = 0;
		pAlmScanType->genEvtTpNumInScanType = 0;
		
		listInit(&(pAlmScanType->evtScanSubTCharaQuen));	
		listInit(&(pAlmScanType->evtTpsOfScanTpQuen));
		pAlmScanType->evtSubTpNumInScanType = 0;
		pAlmScanType->evtTpNumInScanType = 0;

		pAlmScanType->evtUsePag = 0;
		for(l=0; l<GPN_EVT_PAGE_NUM; l++)
		{
			pAlmScanType->scanTypeEvtMask[l] = 0;
			pAlmScanType->evtScreInfo[l] = 0;
			pAlmScanType->evtReptInfo[l] = 0;
		}

		genAlmTypeNum = 0;
		pGenAlmTpNode = (stAlmTpNode *)listFirst(&(pAlmScanType->genAlmTpsOfScanTpQuen));
		while((pGenAlmTpNode != NULL) && (genAlmTypeNum < pAlmScanType->genAlmTpNumInScanType))
		{
			genEvtTypeNum = 0;
			pgenEvtTpNode = (stEvtTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genEvtTypeQuen));
			while((pgenEvtTpNode != NULL)&&(genEvtTypeNum < pgstAlmTypeWholwInfo->genEvtTypeNum))
			{
				if((pgenEvtTpNode->pEvtTpStr->evtType & GPN_EVENT_TYPE_MASK) == pGenAlmTpNode->pAlmTpStr->almType)
				{
					/*find mem space */
					pgenEvtTpNodeTmp = pgstAlmTypeWholwInfo->pEvtTpIndexPoolforGenScanTpQ + i;
					/*node index link */
					pgenEvtTpNodeTmp->pEvtTpStr = pgenEvtTpNode->pEvtTpStr;

					listAdd(&(pAlmScanType->genEvtTpsOfScanTpQuen), (NODE *)pgenEvtTpNodeTmp);
					pAlmScanType->genEvtTpNumInScanType++;

					i++;
					
					/*evt sub type to alarm scantype*/
					genEvtSubTypeNum = 0;
					pgenEvtSubTpNode = (stEvtSubTpNode *)listFirst(&(pgenEvtTpNode->pEvtTpStr->evtSubTypeCharaQuen));
					while((pgenEvtSubTpNode != NULL)&&(genEvtSubTypeNum < pgenEvtTpNode->pEvtTpStr->evtNumInType))
					{
						/*find mem space */
						pgenEvtSubTpNodeTmp = pgstAlmTypeWholwInfo->pEvtSubTpIndexPoolforGenScanTpQ + j;
						/*node index link */
						pgenEvtSubTpNodeTmp->pEvtSubTpStr = pgenEvtSubTpNode->pEvtSubTpStr;
						
						listAdd(&(pAlmScanType->genEvtScanSubTCharaQuen), (NODE *)(pgenEvtSubTpNodeTmp));
						pAlmScanType->genEvtSubTpNumInScanType++;

						j++;

						genEvtSubTypeNum++;
						pgenEvtSubTpNode = (stEvtSubTpNode *)listNext((NODE *)(pgenEvtSubTpNode));
					}

					if(pAlmScanType->genEvtSubTpNumInScanType > 32)
					{
						GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s:event Sub type Num OverSize err\n\r",
							__FUNCTION__);
						gpnLog(GPN_LOG_L_ALERT, "%s:event Sub type Num OverSize err\n\r",
							__FUNCTION__);
						return GPN_EVT_GEN_ERR;
					}

					break;
				}
				
				genEvtTypeNum++;
				pgenEvtTpNode = (stEvtTpNode *)listNext((NODE *)(pgenEvtTpNode));
			}
			
			evtTypeNum = 0;
			pEvtType = (stEvtTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->evtTypeQuen));
			while((pEvtType != NULL)&&(evtTypeNum < pgstAlmTypeWholwInfo->evtTypeNum))
			{
				if((pEvtType->evtType & GPN_EVENT_TYPE_MASK) == pGenAlmTpNode->pAlmTpStr->almType)
				{
					/*find mem space */
					pEvtTpNode = pgstAlmTypeWholwInfo->pEvtTpIndexPoolforScanTpQ + k;
					/*node index link */
					pEvtTpNode->pEvtTpStr = pEvtType;
					
					listAdd(&(pAlmScanType->evtTpsOfScanTpQuen), (NODE *)pEvtTpNode);
					pAlmScanType->evtTpNumInScanType++;

					/*pEvtTpNode: evtTp to alarm scantype*/
					k++;
					
					/*evt sub type to alarm scantype*/
					evtSubTypeNum = 0;
					pEvtSubTpNode = (stEvtSubTpNode *)listFirst(&(pEvtType->evtSubTypeCharaQuen));
					while((pEvtSubTpNode != NULL)&&(evtSubTypeNum < pEvtType->evtNumInType))
					{
						/*find mem space : use pEvtSubTpStr Node */
						/*node index link : not do this */
						
						listAdd(&(pAlmScanType->evtScanSubTCharaQuen), (NODE *)(pEvtSubTpNode->pEvtSubTpStr));
						pAlmScanType->evtSubTpNumInScanType++;
						
						evtSubTypeNum++;
						pEvtSubTpNode = (stEvtSubTpNode *)listNext((NODE *)(pEvtSubTpNode));
					}

					break;
				}
				
				evtTypeNum++;
				pEvtType = (stEvtTypeDef *)listNext((NODE *)(pEvtType));
			}
			
			genAlmTypeNum++;
			pGenAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pGenAlmTpNode));
		}

		/* event num in scan type check */
		if(pAlmScanType->genEvtSubTpNumInScanType > GPN_EVT_MAX_SCAN_EVT)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almScanType(%08x) have too mutch subEvtType(%d)\n\r",\
				__FUNCTION__, pAlmScanType->almScanType,\
				pAlmScanType->genEvtSubTpNumInScanType);
			gpnLog(GPN_LOG_L_ALERT, "%s : almScanType(%08x) have too mutch subEvtType(%d)\n\r",\
				__FUNCTION__, pAlmScanType->almScanType,\
				pAlmScanType->genEvtSubTpNumInScanType);
			return GPN_EVT_GEN_ERR;
		}
		
		almScanTypeNum++;
		pAlmScanType = (stAlmScanTypeDef *)listNext((NODE *)(pAlmScanType));
	}

	return GPN_EVT_GEN_OK;
}
UINT32 gpnEvtEvtSubTypeSysIdentifyCreat(void)
{
	UINT32 page;
	UINT32 bitOrder;
	UINT32 sysId;
	UINT32 evtBitSize;
	UINT32 almScanTypeNum;
	stAlmScanTypeDef *pAlmScanType;
	UINT32 evtSubTpNum;
	stEvtSubTpNode *pgenEvtSubTpNode;
	
	almScanTypeNum = 0;
	pAlmScanType = (stAlmScanTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almScanTypeQuen));
	while((pAlmScanType != NULL) && (almScanTypeNum < pgstAlmTypeWholwInfo->almScanTypeNum))
	{
		evtSubTpNum = 0;
		pgenEvtSubTpNode = (stEvtSubTpNode *)listFirst(&(pAlmScanType->genEvtScanSubTCharaQuen));
		while((pgenEvtSubTpNode != NULL)&&(evtSubTpNum<pAlmScanType->genEvtSubTpNumInScanType))
		{
			/* creat evt sub type sys identify */
			/* screen compiler warning: left shift count >= width of type*/
			evtBitSize = GPN_EVT_PAGE_BIT_SIZE;
			page = evtSubTpNum / evtBitSize;
			bitOrder = evtSubTpNum % evtBitSize;
			sysId = (page << evtBitSize) | (1U << bitOrder);

			pgenEvtSubTpNode->pEvtSubTpStr->evtUseBitPosition = evtSubTpNum;
			pgenEvtSubTpNode->pEvtSubTpStr->sysUseIdentify = sysId;

			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : in scanType(%08x) evtSubType(%08x) sysUseIdentify (%08x)\n\r",\
				__FUNCTION__, pAlmScanType->almScanType,\
				pgenEvtSubTpNode->pEvtSubTpStr->evtSubType,\
				pgenEvtSubTpNode->pEvtSubTpStr->sysUseIdentify);
			/*printf("%s : in restType(%08x) almSubType(%08x) sysUseIdentify (%08x)\n\r",\
				__FUNCTION__, pAlmScanType->almScanType,\
				pgenEvtSubTpNode->pEvtSubTpStr->evtSubType,\
				pgenEvtSubTpNode->pEvtSubTpStr->sysUseIdentify);*/

			evtSubTpNum++;
			pgenEvtSubTpNode = (stEvtSubTpNode *)listNext((NODE *)(pgenEvtSubTpNode));
		}
		
		almScanTypeNum++;
		pAlmScanType = (stAlmScanTypeDef *)listNext((NODE *)(pAlmScanType));
	}
	
	return GPN_EVT_GEN_OK;
}
UINT32 gpnEvtScanTypeEvtAtrribInit(void)
{
	UINT32 page;
	UINT32 evtBitSize;
	UINT32 almScanTypeNum;
	stAlmScanTypeDef *pAlmScanType;
	UINT32 evtSubTpNum;
	stEvtSTCharacterDef *pEvtSubTpStr;
	
	almScanTypeNum = 0;
	pAlmScanType = (stAlmScanTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almScanTypeQuen));
	while((pAlmScanType != NULL) && (almScanTypeNum < pgstAlmTypeWholwInfo->almScanTypeNum))
	{
		pAlmScanType->evtUsePag = pAlmScanType->genEvtSubTpNumInScanType/GPN_EVT_PAGE_BIT_SIZE;
		if((pAlmScanType->genEvtSubTpNumInScanType%GPN_EVT_PAGE_BIT_SIZE) != 0)
		{
			pAlmScanType->evtUsePag++;
		}
		
		evtSubTpNum = 0;
		pEvtSubTpStr = (stEvtSTCharacterDef *)listFirst(&(pAlmScanType->evtScanSubTCharaQuen));
		while((pEvtSubTpStr != NULL)&&(evtSubTpNum<pAlmScanType->evtSubTpNumInScanType))
		{
			/*atrrib init*/
			/*set ScanType event Mask : same event check */
			/* screen compiler warning: left shift count >= width of type*/
			evtBitSize = GPN_EVT_PAGE_BIT_SIZE;
			if(evtBitSize == 32)
			{
				page = 0;
			}
			else
			{
				page = pEvtSubTpStr->sysUseIdentify >> evtBitSize;
			}
			if(pAlmScanType->scanTypeEvtMask[page] & pEvtSubTpStr->sysUseIdentify)
			{
				GPN_EVT_TYPE_PRINT(GPN_EVT_TYPE_EGP, "%s:set scanType event Mask err: same evtSubTp:%08x\n\r",
					__FUNCTION__, pEvtSubTpStr->evtSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s:set scanType event Mask err: same evtSubTp:%08x\n\r",
					__FUNCTION__, pEvtSubTpStr->evtSubType);
				return GPN_EVT_GEN_ERR;
			}
			else
			{
				pAlmScanType->scanTypeEvtMask[page] |= pEvtSubTpStr->sysUseIdentify;
			}
			/*printf("%s : event Type %08x sysId %08x page %d pAlmScanType->evtUsePag %d\n\r",
				__FUNCTION__, pEvtSubTpStr->evtSubType,\
				pEvtSubTpStr->sysUseIdentify, page,\
				pAlmScanType->evtUsePag);*/
			if(pEvtSubTpStr->isScreen == DIS)
			{
				pAlmScanType->evtScreInfo[page] |= pEvtSubTpStr->sysUseIdentify;
			}
			if(pEvtSubTpStr->isReport == EN)
			{
				pAlmScanType->evtReptInfo[page] |= pEvtSubTpStr->sysUseIdentify;
			}

			evtSubTpNum++;
			pEvtSubTpStr = (stEvtSTCharacterDef *)listNext((NODE *)(pEvtSubTpStr));
		}
		
		almScanTypeNum++;
		pAlmScanType = (stAlmScanTypeDef *)listNext((NODE *)(pAlmScanType));
	}
	
	return GPN_EVT_GEN_OK;
}
