UINT32 gpnAlmRestSpecialArrayBitIdGet(UINT32 *bitId, UINT32 arraySize, UINT32 bitPosit)
{
	UINT32 pag;
	UINT32 bit;
	
	if(	(bitId == NULL)||(arraySize > GPN_ALM_PAGE_NUM)||
		(bitPosit >= arraySize*GPN_ALM_PAGE_BIT_SIZE))
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : BitIdGet severity error\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : BitIdGet severity error\n\r",\
			__FUNCTION__);
		return GPN_ALM_GEN_NO;
	}
	else
	{
		pag = bitPosit / GPN_ALM_PAGE_BIT_SIZE;
		bit = bitPosit % GPN_ALM_PAGE_BIT_SIZE;

		*bitId = 0;
		*bitId |= (1U <<bit);
		*bitId |=  pag << GPN_ALM_PAGE_BIT_SIZE;
		
		return GPN_ALM_GEN_YES;
	}
}
