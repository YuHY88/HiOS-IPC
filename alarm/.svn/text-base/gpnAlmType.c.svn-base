				__FUNCTION__, genAlmType, genAlmSubType);
			
			return GPN_ALM_GEN_ERR;
		}
	}

	*pAmTypeNum = genAlmTypeNum;
	*pAmSubTypeNum = genAlmSubTypeNum;
	
	/*printf("%s : genAlmTypeNum(%d) genAlmSubTypeNum(%d)!\n\r",\
		__FUNCTION__, genAlmTypeNum, genAlmSubTypeNum);*/
	
	return GPN_ALM_GEN_OK;
}

UINT32 gpnAlmStructGenAlmTypeInit(void)
{
	UINT32 i;
	UINT32 j;
	UINT32 k;
	UINT32 l;

	UINT32 genAlmSubTypeNum;
	UINT32 genAlmTypeNum;
	
	stAlmRTCharacterDef *pgstAlmTypeTemp;
	
	stAlmTypeDef *pAlmType;
	stAlmSTCharacterDef *pAlmSubType;

	stAlmSubTpNode *pAlmSubTpNode;
	/*stAlmSubTpNode *pQueAlmSubTpNode;*/
	stAlmTpNode *pAlmTpNode;
	stAlmTpNode *pQueAlmTpNode;
	
	genAlmTypeNum = 0;
	genAlmSubTypeNum = 0;
	gpnAlmGenBaseAlmTypeNumGet(&genAlmTypeNum, &genAlmSubTypeNum);
	/***************************************************************************/
	/*                                                    mem view                                                                 */
	/***************************************************************************/
	/* 1  alarm type info struct                            stAlmTypeDef                  genAlmTypeNum      */
	/* 2  sub alarm type info struct                      stAlmSTCharacterDef        genAlmSubTypeNum*/
	/* 3  alm type node for gen alm type              stAlmTpNode                   genAlmTypeNum      */
	/* 4  sub alm type node for gen sub alm type  stAlmSubTpNode              genAlmTypeNum      */
	/* 5  sub alm type node for gen alm type        stAlmSubTpNode              genAlmTypeNum      */
	/****************************************************************************/
	i = genAlmTypeNum*sizeof(stAlmTypeDef) +/*alarm types info size*/\
		genAlmSubTypeNum*sizeof(stAlmSTCharacterDef) +/*sub types info size*/\
		1*genAlmTypeNum*sizeof(stAlmTpNode) +/*alarm type to general alarm type*/\
		2*genAlmSubTypeNum*sizeof(stAlmSubTpNode)/*sub type to general alarm type/general alarm sub type*/;
	
	pgstAlmTypeWholwInfo->pAlmTypeNodePool = (stAlmTypeDef*)malloc(i);
	if(pgstAlmTypeWholwInfo->pAlmTypeNodePool == NULL)
	{
		perror("gpnAlmStructGenAlmTypeInit malloc : ");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_ALM_GEN_ERR;
	}
	memset((UINT8 *)pgstAlmTypeWholwInfo->pAlmTypeNodePool, 0, i);
	
	pgstAlmTypeWholwInfo->pAlmSubTypeNodePool =\
		(stAlmSTCharacterDef*)((UINT8*)pgstAlmTypeWholwInfo->pAlmTypeNodePool +\
		genAlmTypeNum*sizeof(stAlmTypeDef));
	
	pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenAlmTpQ =\
		(stAlmTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pAlmSubTypeNodePool) +\
		genAlmSubTypeNum*sizeof(stAlmSTCharacterDef));
	
	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenSubTpQ =\
		(stAlmSubTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenAlmTpQ) +\
		genAlmTypeNum*sizeof(stAlmTpNode));
	
	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenAlmTpQ =\
		(stAlmSubTpNode*)((UINT8*)(pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenSubTpQ) +\
		genAlmSubTypeNum*sizeof(stAlmSubTpNode));

	/*get general alarm type and general alarm sub type */
	pgstAlmTypeTemp = gstAlmTypeTemplate;
	/*get table line */
	genAlmSubTypeNum = sizeof(gstAlmTypeTemplate)/sizeof(gstAlmTypeTemplate[0]);
	for(i=0,j=0,k=0;i<genAlmSubTypeNum;i++,pgstAlmTypeTemp++)
	{
		if(IS_GPN_ALM_SUB_TYPE(pgstAlmTypeTemp->almSubType))
		{
			/*find mem space */
			pAlmSubType = pgstAlmTypeWholwInfo->pAlmSubTypeNodePool + j;
			pAlmSubTpNode = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenSubTpQ + j;
			/*node index link */
			pAlmSubTpNode->pAlmSubTpStr   = pAlmSubType;

			/*init sub alarm type info */
			pAlmSubType->almSubType 	  = pgstAlmTypeTemp->almSubType;
			
			pAlmSubType->almFunc		  = pgstAlmTypeTemp->almFunc;
			pAlmSubType->almRank		  = pgstAlmTypeTemp->almRank;
			pAlmSubType->isScreen		  = pgstAlmTypeTemp->isScreen;
			pAlmSubType->isInver		  = DIS;
			
			pAlmSubType->isFilted		  = pgstAlmTypeTemp->isFilted;
			pAlmSubType->isReport		  = pgstAlmTypeTemp->isReport;
			pAlmSubType->lampNotify 	  = pgstAlmTypeTemp->lampNotify;
			pAlmSubType->isFillup		  = pgstAlmTypeTemp->isFillup;

			pAlmSubType->almLevel         = 0;
			pAlmSubType->isRecord		  = pgstAlmTypeTemp->isRecord;
			pAlmSubType->almUseBitPosition= 0;
			pAlmSubType->devOrderInScanTp = 0;
			
			pAlmSubType->evtDetailsLen	  = pgstAlmTypeTemp->evtDetailsLen;
			pAlmSubType->sysUseIdentify   = 0;
			pAlmSubType->almName		  = pgstAlmTypeTemp->almName;
			for(l=0;l<GPN_ALM_PAGE_NUM;l++)
			{
				pAlmSubType->restrainMask[l] = 0;
			}
			
			listAdd(&(pgstAlmTypeWholwInfo->genAlmSubTypeQuen), (NODE *)pAlmSubTpNode);
			pgstAlmTypeWholwInfo->genAlmSubTypeNum++;

			/*find mem space */
			/*pAlmSubType = pgstAlmTypeWholwInfo->pAlmSubTypeNodePool + j;*/
			pAlmSubTpNode = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenAlmTpQ + j;
			/*node index link */
			pAlmSubTpNode->pAlmSubTpStr = pAlmSubType;

			genAlmTypeNum = 0;
			pQueAlmTpNode = (stAlmTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genAlmTypeQuen));
			while((pQueAlmTpNode != NULL)&&(genAlmTypeNum < pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				if(pQueAlmTpNode->pAlmTpStr->almType == (pAlmSubTpNode->pAlmSubTpStr->almSubType & GPN_ALM_TYPE_MASK))
				{
					listAdd(&(pQueAlmTpNode->pAlmTpStr->genAlmSubTypeCharaQuen), (NODE *)pAlmSubTpNode);
					pQueAlmTpNode->pAlmTpStr->genAlmNumInType++;
					break;
				}
				genAlmTypeNum++;
				pQueAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pQueAlmTpNode));
			}
			if((pQueAlmTpNode == NULL)||(genAlmTypeNum >= pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : genAlmSubType(%08x) to genAlmType err\n\r",\
					__FUNCTION__, pAlmSubTpNode->pAlmSubTpStr->almSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : genAlmSubType(%08x) to genAlmType err\n\r",\
					__FUNCTION__, pAlmSubTpNode->pAlmSubTpStr->almSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			j++;
		}
		else
		{
			/*find mem space */
			pAlmType = pgstAlmTypeWholwInfo->pAlmTypeNodePool + k;
			pAlmTpNode = pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenAlmTpQ + k;
			/*node index link */
			pAlmTpNode->pAlmTpStr = pAlmType;

			/*init alarm type info */
			pAlmType->almType		= (pgstAlmTypeTemp->almSubType & GPN_ALM_TYPE_MASK);
			pAlmType->almNumInType  = 0;
			listInit(&(pAlmType->almSubTypeCharaQuen));
			pAlmType->genAlmNumInType  = 0;
			listInit(&(pAlmType->genAlmSubTypeCharaQuen));
			pAlmType->almTypeName	= pgstAlmTypeTemp->almName;
			
			/*add in genAlmTypeQuen*/
			listAdd(&(pgstAlmTypeWholwInfo->genAlmTypeQuen), (NODE *)pAlmTpNode);
			pgstAlmTypeWholwInfo->genAlmTypeNum++;
			
			k++;
		}
	}
	/*printf("%s : genAlmTypeNum(%d) genAlmSubTypeNum(%d)!\n\r",\
		__FUNCTION__, pgstAlmTypeWholwInfo->genAlmTypeNum,\
		pgstAlmTypeWholwInfo->genAlmSubTypeNum);*/
	return GPN_ALM_GEN_OK;
}
UINT32 gpnAlmDevBaseAlmTypeNumGet(UINT32 *pAmTypeNum, UINT32 *pAmSubTypeNum)
{
	UINT32 i;
	//UINT32 j;
	UINT32 tableLine;
	UINT32 devType;
	UINT32 almType;
	UINT32 almSubType;
	UINT32 almTypeEn;
	UINT32 almTypeNum;
	UINT32 almSubTypeNum;
	stAlmSubTypeBaseDev *pgAlmDevSupTemp;

	gpnDevApiGetDevType(&devType, NULL, 0);
	gpnLog(GPN_LOG_L_ALERT, "%s : devType %d\n",\
		__FUNCTION__, devType);
	
	/*gAlmSubTypeBaseDevTemplate table check */
	pgAlmDevSupTemp = gAlmSubTypeBaseDevTemplate;
	/*get table line */
	tableLine = sizeof(gAlmSubTypeBaseDevTemplate)/sizeof(gAlmSubTypeBaseDevTemplate[0]);
	
	almTypeEn = GPN_ALM_GEN_DISABLE;
	almType = GPN_ALM_SUB_TYPE_NULL;
	almSubType = GPN_ALM_TYPE_NULL;
	almTypeNum = 0;
	almSubTypeNum = 0;
	for(i=0;i<tableLine;i++,pgAlmDevSupTemp++)
	{
		/* template format check : alarm type first, alarm sub type followed */
		if(IS_GPN_ALM_TYPE(pgAlmDevSupTemp->almSubType))
		{
			almType = pgAlmDevSupTemp->almSubType;
			almSubType = pgAlmDevSupTemp->almSubType;
			if(pgAlmDevSupTemp->devSup.support[devType] == EN)
			{
				almTypeNum++;
				almTypeEn = GPN_ALM_GEN_ENABLE;
			}
			else
			{
				almTypeEn = GPN_ALM_GEN_DISABLE;
			}
		}
		else
		{
			almSubType = pgAlmDevSupTemp->almSubType;
			if( (pgAlmDevSupTemp->devSup.support[devType] == EN) &&
				(almTypeEn == GPN_ALM_GEN_ENABLE) )
			{
				almSubTypeNum++;
			}
		}

		if(almType != (almSubType & GPN_ALM_TYPE_MASK))
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : gAlmSubTypeBaseDevTemplate format err, almType(%08x) almSubType(%08x)!\n\r",\
				__FUNCTION__, almType, almSubType);
			gpnLog(GPN_LOG_L_ALERT, "%s : gAlmSubTypeBaseDevTemplate format err, almType(%08x) almSubType(%08x)!\n\r",\
				__FUNCTION__, almType, almSubType);
			
			return GPN_ALM_GEN_ERR;
		}
	}

	*pAmTypeNum = almTypeNum;
	*pAmSubTypeNum = almSubTypeNum;
	
	/*printf("%s : almTypeNum(%d) almSubTypeNum(%d)!\n\r",\
		__FUNCTION__, almTypeNum, almSubTypeNum);*/
	
	return GPN_ALM_GEN_OK;
}

UINT32 gpnAlmStructDevBaseAlmTypeInit(void)
{
	UINT32 i;
	UINT32 j;
	UINT32 k;

	UINT32 devType;
	
	UINT32 almSubTypeNum;
	UINT32 almTypeNum;
	UINT32 genAlmSubTypeNum;
	UINT32 genAlmTypeNum;
	UINT32 devAlmSubTypeNum;
	UINT32 devAlmTypeNum;

	stAlmSubTypeBaseDev *pgstAlmSubTypeBaseDevTemp;

	stAlmTypeDef *pAlmType;

	stAlmSubTpNode *pAlmSubTpNode;
	stAlmSubTpNode *pGenAlmSubTpNode;
	stAlmTpNode *pGenAlmTpNode;

	stAlmSubTpNode *pDevAlmSubTpNode;
	stAlmTypeDef *pDevAlmType;

	almSubTypeNum = 0;
	gpnAlmDevBaseAlmTypeNumGet(&almTypeNum, &almSubTypeNum);
	/***************************************************************************/
	/*                                                    mem view                                                                 */
	/***************************************************************************/
	/* 1  sub alm type node for devbase sub alm type  stAlmSubTpNode            almSubTypeNum */
	/* 2  sub alm type node for devbase alm type        stAlmSubTpNode            almSubTypeNum */
	/****************************************************************************/
	i = 2*almSubTypeNum*sizeof(stAlmSubTpNode)/*sub type to devbase sub alarm type & devbase alarm type*/;

	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforSubTpQ = (stAlmSubTpNode*)malloc(i);
	if(pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforSubTpQ == NULL)
	{
		perror("gpnAlmStructDevBaseAlmTypeInit malloc : ");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_ALM_GEN_ERR;
	}
	memset((UINT8 *)pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforSubTpQ, 0, i);
	
	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforAlmTpQ =\
		(stAlmSubTpNode*)((UINT8*)pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforSubTpQ +\
		almSubTypeNum*sizeof(stAlmSubTpNode));

	/*get Dev type */
	gpnDevApiGetDevType(&devType, NULL, 0);
	/*gAlmSubTypeBaseDevTemplate table check */
	pgstAlmSubTypeBaseDevTemp = gAlmSubTypeBaseDevTemplate;
	/*get table line */
	almSubTypeNum = sizeof(gAlmSubTypeBaseDevTemplate)/sizeof(gAlmSubTypeBaseDevTemplate[0]);
	
	for(i=0,j=0,k=0;i<almSubTypeNum;i++,pgstAlmSubTypeBaseDevTemp++)
	{
		zlog_debug ("%s[%d] : almSubType[0x%08x], support[%d]\n", __func__, __LINE__,
			pgstAlmSubTypeBaseDevTemp->almSubType, 
			pgstAlmSubTypeBaseDevTemp->devSup.support[devType]);
		if(pgstAlmSubTypeBaseDevTemp->devSup.support[devType] == DIS)
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_CUP, "%s : dev(%d) do not support almType(%08x)\n\r",\
					__FUNCTION__, devType, pgstAlmSubTypeBaseDevTemp->almSubType);
			continue;
		}
		if(IS_GPN_ALM_SUB_TYPE(pgstAlmSubTypeBaseDevTemp->almSubType))
		{
			/*check 1 : check alarm type in devAlarm type quen, if not , err happen or alarm type not sup */
			almTypeNum = 0;
			pAlmType = (stAlmTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almTypeQuen));
			while((pAlmType != NULL)&&(almTypeNum < pgstAlmTypeWholwInfo->almTypeNum))
			{
				if(pAlmType->almType == (pgstAlmSubTypeBaseDevTemp->almSubType & GPN_ALM_TYPE_MASK))
				{
					/*find alarm type in devSubAlarm type Quen*/
					break;
				}
				almTypeNum++;
				pAlmType = (stAlmTypeDef *)listNext((NODE *)(pAlmType));
			}
			if((pAlmType == NULL)||(almTypeNum >= pgstAlmTypeWholwInfo->almTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almSubType(%08x)'s almType not in devAlmSubTypeQuen, error or not support!\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : almSubType(%08x)'s almType not in devAlmSubTypeQuen, error or not support!\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				continue;
			}
			
			/*check 2 : check sub alarm type in genSubAlarm type quen, if not, err happen */
			genAlmSubTypeNum = 0;
			pGenAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genAlmSubTypeQuen));
			while((pGenAlmSubTpNode != NULL)&&(genAlmSubTypeNum < pgstAlmTypeWholwInfo->genAlmSubTypeNum))
			{
				if(pGenAlmSubTpNode->pAlmSubTpStr->almSubType == pgstAlmSubTypeBaseDevTemp->almSubType)
				{
					/*find sub alarm type in genSubAlarm type Quen*/
					break;
				}
				genAlmSubTypeNum++;
				pGenAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pGenAlmSubTpNode));
			}
			if((pGenAlmSubTpNode == NULL)||(genAlmSubTypeNum >= pgstAlmTypeWholwInfo->genAlmSubTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almSubType(%08x) not in genAlmSubTypeQuen err\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : almSubType(%08x) not in genAlmSubTypeQuen err\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			/*check 3 : check alarm type in devAlarm type quen, if already add , err happen*/
			devAlmSubTypeNum = 0;
			pDevAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pgstAlmTypeWholwInfo->almSubTypeQuen));
			while((pDevAlmSubTpNode != NULL)&&(devAlmSubTypeNum < pgstAlmTypeWholwInfo->almSubTypeNum))
			{
				if(pDevAlmSubTpNode->pAlmSubTpStr->almSubType == pgstAlmSubTypeBaseDevTemp->almSubType)
				{
					GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almType(%08x) already in almTypeQuen err\n\r",\
						__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
					gpnLog(GPN_LOG_L_ALERT, "%s : find almType(%08x) already in almTypeQuen err\n\r",\
						__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
					return GPN_ALM_GEN_ERR;
				}
				devAlmSubTypeNum++;
				pDevAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pDevAlmSubTpNode));
			}

			/* add alarm sub type in almSubTypeQuen*/
			/*find mem space */
			pAlmSubTpNode = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforSubTpQ + j;
			/*node index link */
			pAlmSubTpNode->pAlmSubTpStr = pGenAlmSubTpNode->pAlmSubTpStr;

			zlog_debug ("%s[%d] : add subType[0x%08x], pAlmSubTpNode[%p], head[%p]\n", __func__, __LINE__,
				pAlmSubTpNode->pAlmSubTpStr->almSubType, pAlmSubTpNode, 
				(stAlmSubTpNode *)listFirst(&(pgstAlmTypeWholwInfo->almSubTypeQuen)));
			listAdd(&(pgstAlmTypeWholwInfo->almSubTypeQuen), (NODE *)pAlmSubTpNode);
			pgstAlmTypeWholwInfo->almSubTypeNum++;

			/* add alarm sub type in almSubTypeCharaQuen*/
			/*find mem space */
			pAlmSubTpNode = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforAlmTpQ + j;
			/*node index link */
			pAlmSubTpNode->pAlmSubTpStr = pGenAlmSubTpNode->pAlmSubTpStr;
			
			almTypeNum = 0;
			pAlmType = (stAlmTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almTypeQuen));
			while((pAlmType != NULL)&&(almTypeNum < pgstAlmTypeWholwInfo->almTypeNum))
			{
				if(pAlmType->almType == (pgstAlmSubTypeBaseDevTemp->almSubType & GPN_ALM_TYPE_MASK))
				{
					listAdd(&(pAlmType->almSubTypeCharaQuen), (NODE *)pAlmSubTpNode);
					pAlmType->almNumInType++;
					break;
				}
				almTypeNum++;
				pAlmType = (stAlmTypeDef *)listNext((NODE *)(pAlmType));
			}
			if((pAlmType == NULL)||(almTypeNum >= pgstAlmTypeWholwInfo->almTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almSubType(%08x) to almType err\n\r",\
					__FUNCTION__, pAlmSubTpNode->pAlmSubTpStr->almSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : almSubType(%08x) to almType err\n\r",\
					__FUNCTION__, pAlmSubTpNode->pAlmSubTpStr->almSubType);
				return GPN_ALM_GEN_ERR;
			}
			
			j++;
		}
		else
		{
			/*check 1 : check alarm type in genAlarm type quen, if not, err happen */
			genAlmTypeNum = 0;
			pGenAlmTpNode = (stAlmTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genAlmTypeQuen));
			while((pGenAlmTpNode != NULL)&&(genAlmTypeNum < pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				if(pGenAlmTpNode->pAlmTpStr->almType == (pgstAlmSubTypeBaseDevTemp->almSubType & GPN_ALM_TYPE_MASK))
				{
					/*find alarm type in genAlarm type Quen*/
					break;
				}
				genAlmTypeNum++;
				pGenAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pGenAlmTpNode));
			}
			if((pGenAlmTpNode == NULL)||(genAlmTypeNum >= pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almType(%08x) not in genAlmTypeQuen err\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				gpnLog(GPN_LOG_L_ALERT, "%s : almType(%08x) not in genAlmTypeQuen err\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
				return GPN_ALM_GEN_ERR;
			}		

			/*check 2 : check alarm type in devAlarm type quen,  if already add , err happen */
			devAlmTypeNum = 0;
			pDevAlmType = (stAlmTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almTypeQuen));
			while((pDevAlmType != NULL)&&(devAlmTypeNum < pgstAlmTypeWholwInfo->almTypeNum))
			{
				if(pDevAlmType->almType == (pgstAlmSubTypeBaseDevTemp->almSubType & GPN_ALM_TYPE_MASK))
				{
					/*find alarm type in devAlarm type Quen*/
					GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s :  almType(%08x) already in almTypeQuen err\n\r",\
					__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
					gpnLog(GPN_LOG_L_ALERT, "%s : almType(%08x) already in almTypeQuen err\n\r",\
						__FUNCTION__, pgstAlmSubTypeBaseDevTemp->almSubType);
					return GPN_ALM_GEN_ERR;
				}
				devAlmTypeNum++;
				pDevAlmType = (stAlmTypeDef *)listNext((NODE *)(pDevAlmType));
			}
			
			listAdd(&(pgstAlmTypeWholwInfo->almTypeQuen), (NODE *)(pGenAlmTpNode->pAlmTpStr));
			pgstAlmTypeWholwInfo->almTypeNum++;
			
			k++;
		}
	}
	/*printf("%s : almTypeNum(%d) almSubTypeNum(%d)!\n\r",\
		__FUNCTION__, pgstAlmTypeWholwInfo->almTypeNum,\
		pgstAlmTypeWholwInfo->almSubTypeNum);*/
	return GPN_ALM_GEN_OK;
}

