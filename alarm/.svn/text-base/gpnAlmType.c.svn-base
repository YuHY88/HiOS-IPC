	UINT32 genAlmSubType;
	UINT32 genAlmTypeNum;
	UINT32 genAlmSubTypeNum;
	stAlmRTCharacterDef *pgstAlmTypeTemp;
	
	/*gstAlmTypeTemplate table check */
	pgstAlmTypeTemp = gstAlmTypeTemplate;
	/*get table line */
	tableLine = sizeof(gstAlmTypeTemplate)/sizeof(gstAlmTypeTemplate[0]);

	genAlmType = GPN_ALM_SUB_TYPE_NULL;
	genAlmSubType = GPN_ALM_TYPE_NULL;
	genAlmTypeNum = 0;
	genAlmSubTypeNum = 0;
	for(i=0;i<tableLine;i++,pgstAlmTypeTemp++)
	{
		/* template format check : alarm type first, alarm sub type followed */
		if(IS_GPN_ALM_TYPE(pgstAlmTypeTemp->almSubType))
		{
			genAlmType = pgstAlmTypeTemp->almSubType;
			genAlmSubType = pgstAlmTypeTemp->almSubType;
			genAlmTypeNum++;
		}
		else
		{
			genAlmSubType = pgstAlmTypeTemp->almSubType;
			genAlmSubTypeNum++;
		}

		if(genAlmType != (genAlmSubType & GPN_ALM_TYPE_MASK))
		{
			GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : gstAlmTypeTemplate format err, almType(%08x) almSubType(%08x)!\n\r",\
				__FUNCTION__, genAlmType, genAlmSubType);
