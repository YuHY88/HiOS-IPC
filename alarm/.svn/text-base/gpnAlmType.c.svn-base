UINT32 gpnAlmStructAlmScanTypeInit(void)
{
	UINT32 i;
	UINT32 k;
	UINT32 l;
	UINT32 m;
	UINT32 n;
	
	UINT32 almScanTypeNum;
	UINT32 almSubTypeNum;
	UINT32 almTypeNum;
	UINT32 genAlmSubTypeNum;
	UINT32 genAlmTypeNum;
	
	stAlmScanTypeDef *pAlmScanType;
	stAlmTypeDef *pAlmType;
	
	stAlmSubTpNode *pAlmSubTpNode;
	stAlmTpNode *pAlmTpNode;
	stAlmSubTpNode *pAlmSubTpNodeExt;
	stAlmTpNode *pAlmTpNodeExt;
	
	almScanTpTemp *pAlmScanTpTemp;
	sysSupAlmScanTpTemp *pgSysSupAlmScanTpTemp;

	/*get table line*/
	gpnAlmAlmScanTypeNumGet(&almScanTypeNum);
	/*******************************************************************************/
	/*                                                    mem view                                                                        */
	/*******************************************************************************/
	/* 1  alm scan type info struct                         stAlmScanTypeDef                almScanTpNum          */
	/* 2  dev alm type node for alm scan type        stAlmTpNode                       dev almTypeNum       */
	/* 3  gen alm type node for alm scan type        stAlmTpNode                       gen almTypeNum       */
	/* 4  gen sub alm type node for alm scan type  stAlmSubTpNode                  gen subAlmTypeNum */
	/********************************************************************************/
	i = almScanTypeNum*sizeof(stAlmScanTypeDef) +/*alm scan type info struct*/\
		pgstAlmTypeWholwInfo->almTypeNum*sizeof(stAlmTpNode) +/*dev alm type node for alm scan type*/\
		pgstAlmTypeWholwInfo->genAlmTypeNum*sizeof(stAlmTpNode) +/*gen alm type node for alm scan type*/\
		pgstAlmTypeWholwInfo->genAlmSubTypeNum*sizeof(stAlmSubTpNode)/*gen sub alm type node for alm scan type*/;
			
	pgstAlmTypeWholwInfo->pAlmScanTypeNodePool = (stAlmScanTypeDef *)malloc(i);
	if(pgstAlmTypeWholwInfo->pAlmScanTypeNodePool == NULL)
	{
		perror("gpnAlmStructAlmScanTypeInit malloc : ");
		gpnLog(GPN_LOG_L_ALERT, "%s line(%d) : malloc err!\n\r",\
			__FUNCTION__, __LINE__);
		return GPN_ALM_GEN_ERR;
	}
	memset((UINT8 *)pgstAlmTypeWholwInfo->pAlmScanTypeNodePool, 0, i);
	
	pgstAlmTypeWholwInfo->pAlmTpIndexPoolforScanTpQ =\
		(stAlmTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmScanTypeNodePool +\
		almScanTypeNum*sizeof(stAlmScanTypeDef));

	pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenScanTpQ =\
		(stAlmTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmTpIndexPoolforScanTpQ +\
		pgstAlmTypeWholwInfo->almTypeNum*sizeof(stAlmTpNode));

	pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenScanTpQ =\
		(stAlmSubTpNode *)((UINT8*)pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenScanTpQ +\
		pgstAlmTypeWholwInfo->genAlmTypeNum*sizeof(stAlmTpNode));	
	
	pgSysSupAlmScanTpTemp = gSysSupAlmScanTpTemp;
	for(i=0,k=0,m=0,n=0;i<almScanTypeNum;i++,pgSysSupAlmScanTpTemp++)
	{
		/*find mem space */
		pAlmScanType = pgstAlmTypeWholwInfo->pAlmScanTypeNodePool + i;
		
		listAdd(&(pgstAlmTypeWholwInfo->almScanTypeQuen), (NODE *)pAlmScanType);
		pgstAlmTypeWholwInfo->almScanTypeNum++;

		/*init alarm scan type info struct */
		pAlmScanTpTemp = pgSysSupAlmScanTpTemp->palmScanTpTemp;
		
		pAlmScanType->almScanType = 			 pAlmScanTpTemp->almScanType;
		listInit(&(pAlmScanType->almScanSubTCharaQuen));
		listInit(&(pAlmScanType->almTpsOfScanTpQuen));
		listInit(&(pAlmScanType->almScanPortObjQuen));
		pAlmScanType->almSubTpNumInScanType =	 0;
		pAlmScanType->almTpNumInScanType =		 0;
		pAlmScanType->almScanQuenPortNum =   	 0;
		pAlmScanType->almSubTpNumInRestType =	 0;

		pAlmScanType->scanTpOpeCtl = 			 pAlmScanTpTemp->almScanIOCtlCode;
		pAlmScanType->scanTpCycle = 			 pAlmScanTpTemp->almScanCycle;

		pAlmScanType->almUsePag =                0;
		for(l=0;l<GPN_ALM_PAGE_NUM;l++)
		{
			pAlmScanType->scanTypeAlmMask[l] =   0;
			pAlmScanType->almScreInfo[l] =       0;
			pAlmScanType->almFiltInfo[l] =       0;
			pAlmScanType->almReptInfo[l] =       0;
			pAlmScanType->almReckInfo[l] =       0;
		}
		
		listInit(&(pAlmScanType->genAlmScanSubTCharaQuen));
		listInit(&(pAlmScanType->genAlmTpsOfScanTpQuen));
		pAlmScanType->genAlmSubTpNumInScanType = 0;
		pAlmScanType->genAlmTpNumInScanType =    0;
		
		pAlmScanType->almScanTypeName = 		 pAlmScanTpTemp->almScanTpName;

		/*find alarm type belong alarm scan type */
		for(l=0;l<pgSysSupAlmScanTpTemp->almTpNumInScanTp;l++,pAlmScanTpTemp++)
		{
			/*find dev alarm type for alarm scan type */
			almTypeNum = 0;
			pAlmType = (stAlmTypeDef *)listFirst(&(pgstAlmTypeWholwInfo->almTypeQuen));
			while((pAlmType != NULL)&&(almTypeNum<pgstAlmTypeWholwInfo->almTypeNum))
			{
				if(pAlmType->almType == (pAlmScanTpTemp->almTpyeMemb & GPN_ALM_TYPE_MASK))
				{	
					/*dev alarm type into alarm scan type */
					if(k < pgstAlmTypeWholwInfo->almTypeNum)
					{
						/*find mem space */
						pAlmTpNode = pgstAlmTypeWholwInfo->pAlmTpIndexPoolforScanTpQ + k;
						/*node index link */
						pAlmTpNode->pAlmTpStr = pAlmType;
						
						listAdd(&(pAlmScanType->almTpsOfScanTpQuen),(NODE *)(pAlmTpNode));
						pAlmScanType->almTpNumInScanType++;
						k++;
					}
					else
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almType into almScanType err k %d almTypeNum %d\n\r",\
							__FUNCTION__, k, pgstAlmTypeWholwInfo->almTypeNum);
						gpnLog(GPN_LOG_L_ALERT, "%s : almType into almScanType err k %d almTypeNum %d\n\r",\
							__FUNCTION__, k, pgstAlmTypeWholwInfo->almTypeNum);
						return GPN_ALM_GEN_ERR;
					}

					/*dev sub alarm type into alarm scan type */
					almSubTypeNum = 0;
					pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmType->almSubTypeCharaQuen));
					while((pAlmSubTpNode != NULL)&&(almSubTypeNum<pAlmType->almNumInType))
					{
						listAdd(&(pAlmScanType->almScanSubTCharaQuen),(NODE *)(pAlmSubTpNode->pAlmSubTpStr));
						pAlmScanType->almSubTpNumInScanType++;
						
						/*告警子类加入扫描类，按加入的顺序记录子类告警在扫描类中的位置*/
						/*生成pAlmSubType中的orderInScanTp信息,从0开始*/
						pAlmSubTpNode->pAlmSubTpStr->devOrderInScanTp = pAlmScanType->almSubTpNumInScanType - 1;

						almSubTypeNum++;
						pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pAlmSubTpNode));
					}
					break;
				}
				
				almTypeNum++;
				pAlmType = (stAlmTypeDef *)listNext((NODE *)(pAlmType));
			}
			if((pAlmType == NULL)||(almTypeNum>=pgstAlmTypeWholwInfo->almTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : dev not support almType(%08x)\n\r",\
					__FUNCTION__, pAlmScanTpTemp->almTpyeMemb);
			}

			/*find gen alarm type for alarm scan type */
			genAlmTypeNum = 0;
			pAlmTpNode = (stAlmTpNode *)listFirst(&(pgstAlmTypeWholwInfo->genAlmTypeQuen));
			while((pAlmTpNode != NULL)&&(genAlmTypeNum<pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				if(pAlmTpNode->pAlmTpStr->almType == (pAlmScanTpTemp->almTpyeMemb & GPN_ALM_TYPE_MASK))
				{	
					pAlmType = pAlmTpNode->pAlmTpStr;
					/*gen alarm type into alarm scan type */
					if(m < pgstAlmTypeWholwInfo->genAlmTypeNum)
					{
						/*find mem space */
						pAlmTpNodeExt = pgstAlmTypeWholwInfo->pAlmTpIndexPoolforGenScanTpQ + m;
						/*node index link */
						pAlmTpNodeExt->pAlmTpStr = pAlmType;
						
						listAdd(&(pAlmScanType->genAlmTpsOfScanTpQuen),(NODE *)(pAlmTpNodeExt));
						pAlmScanType->genAlmTpNumInScanType++;
						m++;
					}
					else
					{
						GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : genAlmType into almScanType err m %d genAlmTypeNum %d\n\r",\
							__FUNCTION__, m, pgstAlmTypeWholwInfo->genAlmTypeNum);
						gpnLog(GPN_LOG_L_ALERT, "%s : genAlmType into almScanType err m %d genAlmTypeNum %d\n\r",\
							__FUNCTION__, m, pgstAlmTypeWholwInfo->genAlmTypeNum);
						return GPN_ALM_GEN_ERR;
					}

					/*gen sub alarm type into alarm scan type */
					genAlmSubTypeNum = 0;
					pAlmSubTpNode = (stAlmSubTpNode *)listFirst(&(pAlmType->genAlmSubTypeCharaQuen));
					while((pAlmSubTpNode != NULL)&&(genAlmSubTypeNum<pAlmType->genAlmNumInType))
					{
						/*find mem space */
						pAlmSubTpNodeExt = pgstAlmTypeWholwInfo->pAlmSubTpIndexPoolforGenScanTpQ + n;
						/*node index link */
						pAlmSubTpNodeExt->pAlmSubTpStr = pAlmSubTpNode->pAlmSubTpStr;
						
						listAdd(&(pAlmScanType->genAlmScanSubTCharaQuen),(NODE *)pAlmSubTpNodeExt);
						pAlmScanType->genAlmSubTpNumInScanType++;

						genAlmSubTypeNum++;
						pAlmSubTpNode = (stAlmSubTpNode *)listNext((NODE *)(pAlmSubTpNode));

						n++;
					}
					break;
				}
				
				genAlmTypeNum++;
				pAlmTpNode = (stAlmTpNode *)listNext((NODE *)(pAlmTpNode));
			}
			if((pAlmTpNode == NULL)&&(genAlmTypeNum>=pgstAlmTypeWholwInfo->genAlmTypeNum))
			{
				GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : almType(%08x) to almScanType(%s) err\n\r",\
					__FUNCTION__, pAlmScanTpTemp->almTpyeMemb,pAlmScanType->almScanTypeName);
				gpnLog(GPN_LOG_L_ALERT, "%s : almType(%08x) to almScanType(%s) err\n\r",\
					__FUNCTION__, pAlmScanTpTemp->almTpyeMemb,pAlmScanType->almScanTypeName);
				return GPN_ALM_GEN_ERR;
			}
		}
	}
	return GPN_ALM_GEN_OK;
}
