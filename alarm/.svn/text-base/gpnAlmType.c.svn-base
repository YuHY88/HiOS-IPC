UINT32 gpnAlmRestSpecialArrayInit(UINT32 *array, UINT32 arraySize)
{
	UINT32 i;

	if(arraySize > GPN_ALM_PAGE_NUM)
	{
		GPN_ALM_TYPE_PRINT(GPN_ALM_TYPE_EGP, "%s : Init severity error\n\r",\
			__FUNCTION__);
		gpnLog(GPN_LOG_L_ALERT, "%s : Init severity error\n\r",\
			__FUNCTION__);
		return GPN_ALM_GEN_ERR;
	}
	for(i=0;i<arraySize;i++)
	{
		array[i] = GPN_ALM_IN_PAGE_BITP | (i<<GPN_ALM_PAGE_BIT_SIZE);
	}
	
	return GPN_ALM_GEN_YES;
}
